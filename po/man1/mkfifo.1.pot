#: mkfifo.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: mkfifo.1:8
msgid "B<mkfifo> [I<OPTION>]... I<NAME>..."
msgstr ""

#. type: SH
#: mkfifo.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: mkfifo.1:12
msgid "Create named pipes (FIFOs) with the given NAMEs."
msgstr ""

#. type: Plain text
#: mkfifo.1:14
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: mkfifo.1:14
#, no-wrap
msgid "B<-m>, B<--mode>=I<MODE>"
msgstr ""

#. type: Plain text
#: mkfifo.1:17
msgid "set file permission bits to MODE, not a=rw - umask"
msgstr ""

#. type: TP
#: mkfifo.1:17
#, no-wrap
msgid "B<-Z>, B<--context>=I<CTX>"
msgstr ""

#. type: Plain text
#: mkfifo.1:20
msgid "set the SELinux security context of each NAME to CTX"
msgstr ""

#. type: TP
#: mkfifo.1:20
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: mkfifo.1:23
msgid "display this help and exit"
msgstr ""

