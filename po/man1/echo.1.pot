#. type: Plain text
#: echo.1:15
msgid "Echo the STRING(s) to standard output."
msgstr ""

#. type: TP
#: echo.1:15
#, no-wrap
msgid "B<-n>"
msgstr ""

#. type: Plain text
#: echo.1:18
msgid "do not output the trailing newline"
msgstr ""

#. type: TP
#: echo.1:18
#, no-wrap
msgid "B<-e>"
msgstr ""

#. type: Plain text
#: echo.1:21
msgid "enable interpretation of backslash escapes"
msgstr ""

#. type: TP
#: echo.1:21
#, no-wrap
msgid "B<-E>"
msgstr ""

#. type: Plain text
#: echo.1:24
msgid "disable interpretation of backslash escapes (default)"
