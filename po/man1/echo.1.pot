#. type: TP
#: echo.1:24
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: echo.1:27
msgid "display this help and exit"
msgstr ""

#. type: TP
#: echo.1:27
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: echo.1:30
msgid "output version information and exit"
