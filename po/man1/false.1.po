msgid "FALSE"
msgstr "FALSE"

#. type: TH
#: false.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: false.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: false.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: false.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: false.1:5
msgid "false - do nothing, unsuccessfully"
msgstr "false - 什么也不做， 返回失败"

#. type: SH
#: false.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: false.1:8
msgid "B<false> [I<ignored command line arguments>]"
msgstr "B<false> [I<ignored command line arguments>]"

#. type: Plain text
#: false.1:11
msgid "B<false> I<OPTION>"
msgstr "B<false> I<选项>"

#. type: SH
#: false.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: false.1:15
msgid "Exit with a status code indicating failure."
msgstr "以指示为失败的状态码退出"

#. type: TP
#: false.1:15
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: false.1:18
msgid "display this help and exit"
msgstr "显示帮助然后退出"

#. type: TP
#: false.1:18
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: false.1:21
msgid "output version information and exit"
msgstr "输出版本信息然后退出"

#. type: Plain text
#: false.1:25
msgid ""
"NOTE: your shell may have its own version of false, which usually supersedes "
"the version described here.  Please refer to your shell's documentation for "
"details about the options it supports."
msgstr ""
"注意： 你的 shell 可能有自己版本的 false 命令， 其通常会取代现在描述的这个版"
"本。 请参阅你的 shell 文档来获取命令支持的选项。"

#. type: SH
#: false.1:25
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: false.1:27
msgid "Written by Jim Meyering."
msgstr "由 Jim Meyering 撰写。"

#. type: SH
#: false.1:27
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: false.1:29
msgid "Report false bugs to bug-coreutils@gnu.org"
msgstr "发送 false 的错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: false.1:31
msgid ""
"GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: false.1:33
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: false.1:35
msgid ""
"Report false translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送 false 的翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

#. type: SH
#: false.1:35
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权声明"

