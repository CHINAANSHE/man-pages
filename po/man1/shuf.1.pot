msgstr ""

#. type: SH
#: shuf.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: shuf.1:5
msgid "shuf - generate random permutations"
msgstr ""

#. type: SH
#: shuf.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: shuf.1:8
msgid "B<shuf> [I<OPTION>]... [I<FILE>]"
msgstr ""

#. type: Plain text
#: shuf.1:11
msgid "B<shuf> I<-e >[I<OPTION>]... [I<ARG>]..."
msgstr ""

#. type: Plain text
#: shuf.1:14
msgid "B<shuf> I<-i LO-HI >[I<OPTION>]..."
msgstr ""

#. type: SH
#: shuf.1:14
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: shuf.1:18
msgid "Write a random permutation of the input lines to standard output."
msgstr ""

#. type: Plain text
#: shuf.1:20
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: shuf.1:20
#, no-wrap
msgid "B<-e>, B<--echo>"
msgstr ""

#. type: Plain text
#: shuf.1:23
msgid "treat each ARG as an input line"
msgstr ""

#. type: TP
#: shuf.1:23
#, no-wrap
msgid "B<-i>, B<--input-range>=I<LO-HI>"
msgstr ""

#. type: Plain text
#: shuf.1:26
msgid "treat each number LO through HI as an input line"
msgstr ""

#. type: TP
#: shuf.1:26
#, no-wrap
msgid "B<-n>, B<--head-count>=I<COUNT>"
msgstr ""

#. type: Plain text
#: shuf.1:29
msgid "output at most COUNT lines"
msgstr ""

#. type: TP
#: shuf.1:29
#, no-wrap
msgid "B<-o>, B<--output>=I<FILE>"
msgstr ""

#. type: Plain text
#: shuf.1:32
msgid "write result to FILE instead of standard output"
msgstr ""

#. type: TP
#: shuf.1:32
#, no-wrap
msgid "B<--random-source>=I<FILE>"
msgstr ""

#. type: Plain text
#: shuf.1:35
msgid "get random bytes from FILE"
msgstr ""

#. type: TP
#: shuf.1:35
#, no-wrap
msgid "B<-z>, B<--zero-terminated>"
msgstr ""

#. type: Plain text
#: shuf.1:38
msgid "end lines with 0 byte, not newline"
msgstr ""

#. type: TP
#: shuf.1:38
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: shuf.1:41
msgid "display this help and exit"
msgstr ""

#. type: TP
#: shuf.1:41
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: shuf.1:44
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: shuf.1:46
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

