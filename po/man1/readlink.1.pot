#. type: SH
#: readlink.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: readlink.1:5
msgid "readlink - print resolved symbolic links or canonical file names"
msgstr ""

#. type: SH
#: readlink.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: readlink.1:8
msgid "B<readlink> [I<OPTION>]... I<FILE>..."
msgstr ""

#. type: SH
#: readlink.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: readlink.1:12
msgid "Print value of a symbolic link or canonical file name"
msgstr ""

#. type: TP
#: readlink.1:12
#, no-wrap
msgid "B<-f>, B<--canonicalize>"
msgstr ""

#. type: Plain text
#: readlink.1:17
msgid ""
"canonicalize by following every symlink in every component of the given name "
"recursively; all but the last component must exist"
msgstr ""

#. type: TP
#: readlink.1:17
#, no-wrap
msgid "B<-e>, B<--canonicalize-existing>"
msgstr ""

#. type: Plain text
#: readlink.1:22
msgid ""
"canonicalize by following every symlink in every component of the given name "
"recursively, all components must exist"
msgstr ""

#. type: TP
#: readlink.1:22
#, no-wrap
msgid "B<-m>, B<--canonicalize-missing>"
msgstr ""

#. type: Plain text
#: readlink.1:27
msgid ""
"canonicalize by following every symlink in every component of the given name "
"recursively, without requirements on components existence"
msgstr ""

#. type: TP
#: readlink.1:27
#, no-wrap
msgid "B<-n>, B<--no-newline>"
msgstr ""

#. type: Plain text
#: readlink.1:30
msgid "do not output the trailing delimiter"
msgstr ""

#. type: Plain text
#: readlink.1:32
msgid "B<-q>, B<--quiet>,"
msgstr ""

#. type: TP
#: readlink.1:32
#, no-wrap
msgid "B<-s>, B<--silent>"
msgstr ""

#. type: Plain text
#: readlink.1:35
msgid "suppress most error messages"
msgstr ""

#. type: TP
#: readlink.1:35
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr ""

#. type: Plain text
#: readlink.1:38
msgid "report error messages"
msgstr ""

#. type: TP
#: readlink.1:38
#, no-wrap
msgid "B<-z>, B<--zero>"
msgstr ""

#. type: Plain text
#: readlink.1:41
msgid "separate output with NUL rather than newline"
msgstr ""

#. type: TP
#: readlink.1:41
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: readlink.1:44
msgid "display this help and exit"
msgstr ""

#. type: TP
#: readlink.1:44
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: readlink.1:47
msgid "output version information and exit"
msgstr ""

