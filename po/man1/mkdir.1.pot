#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: mkdir.1:5
msgid "mkdir - make directories"
msgstr ""

#. type: SH
#: mkdir.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: mkdir.1:8
msgid "B<mkdir> [I<OPTION>]... I<DIRECTORY>..."
msgstr ""

#. type: SH
#: mkdir.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: mkdir.1:12
msgid "Create the DIRECTORY(ies), if they do not already exist."
msgstr ""

#. type: Plain text
#: mkdir.1:14
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: mkdir.1:14
#, no-wrap
msgid "B<-m>, B<--mode>=I<MODE>"
msgstr ""

#. type: Plain text
#: mkdir.1:17
msgid "set file mode (as in chmod), not a=rwx - umask"
msgstr ""

#. type: TP
#: mkdir.1:17
#, no-wrap
msgid "B<-p>, B<--parents>"
msgstr ""

#. type: Plain text
#: mkdir.1:20
msgid "no error if existing, make parent directories as needed"
msgstr ""

#. type: TP
#: mkdir.1:20
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr ""

#. type: Plain text
#: mkdir.1:23
msgid "print a message for each created directory"
msgstr ""

#. type: TP
#: mkdir.1:23
#, no-wrap
msgid "B<-Z>, B<--context>=I<CTX>"
msgstr ""

#. type: Plain text
#: mkdir.1:27
msgid "set the SELinux security context of each created directory to CTX"
msgstr ""

#. type: TP
#: mkdir.1:27
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: mkdir.1:30
msgid "display this help and exit"
msgstr ""

#. type: TP
#: mkdir.1:30
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: mkdir.1:33
msgid "output version information and exit"
msgstr ""

