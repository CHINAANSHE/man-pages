#. type: TH
#: pinky.1:2
#, no-wrap
msgid "PINKY"
msgstr ""

#. type: TH
#: pinky.1:2
#, no-wrap
msgid "October 2013"
msgstr ""

#. type: TH
#: pinky.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr ""

#. type: TH
#: pinky.1:2
#, no-wrap
msgid "User Commands"
msgstr ""

#. type: SH
#: pinky.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: pinky.1:5
msgid "pinky - lightweight finger"
msgstr ""

#. type: SH
#: pinky.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: pinky.1:8
msgid "B<pinky> [I<OPTION>]... [I<USER>]..."
msgstr ""

#. type: SH
#: pinky.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: TP
#: pinky.1:10
#, no-wrap
msgid "B<-l>"
msgstr ""

#. type: Plain text
#: pinky.1:13
msgid "produce long format output for the specified USERs"
msgstr ""

#. type: TP
#: pinky.1:13
#, no-wrap
msgid "B<-b>"
msgstr ""

#. type: Plain text
#: pinky.1:16
msgid "omit the user's home directory and shell in long format"
msgstr ""

#. type: TP
#: pinky.1:16
#, no-wrap
msgid "B<-h>"
msgstr ""

#. type: Plain text
#: pinky.1:19
msgid "omit the user's project file in long format"
msgstr ""

#. type: TP
#: pinky.1:19
#, no-wrap
msgid "B<-p>"
msgstr ""

#. type: Plain text
#: pinky.1:22
msgid "omit the user's plan file in long format"
msgstr ""

#. type: TP
#: pinky.1:22
#, no-wrap
msgid "B<-s>"
msgstr ""

#. type: Plain text
#: pinky.1:25
msgid "do short format output, this is the default"
msgstr ""

#. type: TP
#: pinky.1:25
#, no-wrap
msgid "B<-f>"
msgstr ""

#. type: Plain text
#: pinky.1:28
msgid "omit the line of column headings in short format"
msgstr ""

#. type: TP
#: pinky.1:28
#, no-wrap
msgid "B<-w>"
msgstr ""

#. type: Plain text
#: pinky.1:31
msgid "omit the user's full name in short format"
msgstr ""

#. type: TP
#: pinky.1:31
#, no-wrap
msgid "B<-i>"
msgstr ""

#. type: Plain text
#: pinky.1:34
msgid "omit the user's full name and remote host in short format"
msgstr ""

#. type: TP
#: pinky.1:34
#, no-wrap
msgid "B<-q>"
msgstr ""

#. type: Plain text
#: pinky.1:38
msgid "omit the user's full name, remote host and idle time in short format"
msgstr ""

#. type: TP
#: pinky.1:38
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: pinky.1:41
msgid "display this help and exit"
msgstr ""

#. type: TP
#: pinky.1:41
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: pinky.1:44
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: pinky.1:47
msgid ""
"A lightweight 'finger' program; print user information.  The utmp file will "
"be /var/run/utmp."
msgstr ""

#. type: SH
#: pinky.1:47
#, no-wrap
msgid "AUTHOR"
msgstr ""

#. type: Plain text
#: pinky.1:49
msgid "Written by Joseph Arceneaux, David MacKenzie, and Kaveh Ghazi."
msgstr ""

#. type: SH
#: pinky.1:49
#, no-wrap
msgid "REPORTING BUGS"
msgstr ""

#. type: Plain text
#: pinky.1:51
msgid "Report pinky bugs to bug-coreutils@gnu.org"
msgstr ""

#. type: Plain text
#: pinky.1:53
msgid "GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr ""

#. type: Plain text
#: pinky.1:55
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr ""

#. type: Plain text
#: pinky.1:57
msgid ""
"Report pinky translation bugs to "
"E<lt>http://translationproject.org/team/E<gt>"
msgstr ""

