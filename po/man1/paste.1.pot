#. type: SH
#: paste.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: paste.1:5
msgid "paste - merge lines of files"
msgstr ""

#. type: SH
#: paste.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: paste.1:8
msgid "B<paste> [I<OPTION>]... [I<FILE>]..."
msgstr ""

#. type: SH
#: paste.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: paste.1:14
msgid ""
"Write lines consisting of the sequentially corresponding lines from each "
"FILE, separated by TABs, to standard output.  With no FILE, or when FILE is "
"-, read standard input."
msgstr ""

#. type: Plain text
#: paste.1:16
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: paste.1:16
#, no-wrap
msgid "B<-d>, B<--delimiters>=I<LIST>"
msgstr ""

#. type: Plain text
#: paste.1:19
msgid "reuse characters from LIST instead of TABs"
msgstr ""

#. type: TP
#: paste.1:19
#, no-wrap
msgid "B<-s>, B<--serial>"
msgstr ""

#. type: Plain text
#: paste.1:22
msgid "paste one file at a time instead of in parallel"
msgstr ""

#. type: TP
#: paste.1:22
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: paste.1:25
msgid "display this help and exit"
msgstr ""

#. type: TP
#: paste.1:25
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: paste.1:28
msgid "output version information and exit"
msgstr ""

