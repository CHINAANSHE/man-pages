#. type: SH
#: date.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: date.1:5
msgid "date - print or set the system date and time"
msgstr ""

#. type: SH
#: date.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: date.1:8
msgid "B<date> [I<OPTION>]... [I<+FORMAT>]"
msgstr ""

#. type: Plain text
#: date.1:11
msgid "B<date> [I<-u|--utc|--universal>] [I<MMDDhhmm>[[I<CC>]I<YY>][I<.ss>]]"
msgstr ""

#. type: SH
#: date.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: date.1:15
msgid "Display the current time in the given FORMAT, or set the system date."
msgstr ""

#. type: Plain text
#: date.1:17
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: date.1:17
#, no-wrap
msgid "B<-d>, B<--date>=I<STRING>"
msgstr ""

#. type: Plain text
#: date.1:20
msgid "display time described by STRING, not 'now'"
msgstr ""

#. type: TP
#: date.1:20
#, no-wrap
msgid "B<-f>, B<--file>=I<DATEFILE>"
msgstr ""

#. type: Plain text
#: date.1:23
msgid "like B<--date> once for each line of DATEFILE"
msgstr ""

#. type: TP
#: date.1:23
#, no-wrap
msgid "B<-I[TIMESPEC]>, B<--iso-8601>[=I<TIMESPEC>]"
msgstr ""

#. type: Plain text
#: date.1:29
msgid ""
"output date/time in ISO 8601 format.  TIMESPEC='date' for date only (the "
"default), \\&'hours', 'minutes', 'seconds', or 'ns' for date and time to the "
"indicated precision."
msgstr ""

#. type: TP
#: date.1:29
#, no-wrap
msgid "B<-r>, B<--reference>=I<FILE>"
msgstr ""

#. type: Plain text
#: date.1:32
msgid "display the last modification time of FILE"
msgstr ""

#. type: TP
#: date.1:32
#, no-wrap
msgid "B<-R>, B<--rfc-2822>"
msgstr ""

#. type: Plain text
#: date.1:36
msgid ""
"output date and time in RFC 2822 format.  Example: Mon, 07 Aug 2006 12:34:56 "
"B<-0600>"
msgstr ""

#. type: TP
#: date.1:36
#, no-wrap
msgid "B<--rfc-3339>=I<TIMESPEC>"
msgstr ""

#. type: Plain text
#: date.1:43
msgid ""
"output date and time in RFC 3339 format.  TIMESPEC='date', 'seconds', or "
"'ns' for date and time to the indicated precision.  Date and time components "
"are separated by a single space: 2006-08-07 12:34:56-06:00"
msgstr ""

#. type: TP
#: date.1:43
#, no-wrap
msgid "B<-s>, B<--set>=I<STRING>"
msgstr ""

#. type: Plain text
#: date.1:46
msgid "set time described by STRING"
msgstr ""

#. type: TP
#: date.1:46
#, no-wrap
msgid "B<-u>, B<--utc>, B<--universal>"
msgstr ""

#. type: Plain text
#: date.1:49
msgid "print or set Coordinated Universal Time"
msgstr ""

#. type: TP
#: date.1:49
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: date.1:52
msgid "display this help and exit"
msgstr ""

#. type: TP
#: date.1:52
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: date.1:55
msgid "output version information and exit"
msgstr ""

