#. type: Plain text
#: date.1:195
msgid ""
"By default, date pads numeric fields with zeroes.  The following optional "
"flags may follow '%':"
msgstr ""

#. type: TP
#: date.1:195
#, no-wrap
msgid "-"
msgstr ""

#. type: Plain text
#: date.1:198
msgid "(hyphen) do not pad the field"
msgstr ""

#. type: TP
#: date.1:198
#, no-wrap
msgid "_"
msgstr ""

#. type: Plain text
#: date.1:201
msgid "(underscore) pad with spaces"
msgstr ""

#. type: TP
#: date.1:201
#, no-wrap
msgid "0"
msgstr ""

#. type: Plain text
#: date.1:204
msgid "(zero) pad with zeros"
msgstr ""

#. type: TP
#: date.1:204
#, no-wrap
msgid "^"
msgstr ""

#. type: Plain text
#: date.1:207
msgid "use upper case if possible"
msgstr ""

#. type: TP
