#. type: TH
#: uname.1:2
#, no-wrap
msgid "UNAME"
msgstr "UNAME"

#. type: TH
#: uname.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: uname.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: uname.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: uname.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: uname.1:5
msgid "uname - print system information"
msgstr "uname - 打印系统信息"

#. type: SH
#: uname.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: uname.1:8
msgid "B<uname> [I<OPTION>]..."
msgstr "B<uname> [I<选项>]..."

#. type: SH
#: uname.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: uname.1:12
msgid "Print certain system information.  With no OPTION, same as B<-s>."
msgstr "打印某些系统信息。不带选项时与 B<-s>相同。"

#. type: TP
#: uname.1:12
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr "B<-a>, B<--all>"

#. type: Plain text
#: uname.1:16
msgid ""
"print all information, in the following order, except omit B<-p> and B<-i> "
"if unknown:"
msgstr "以如下的顺序打印所有的信息，除了忽略掉B<-p> 和 B<-i>未知的情况："

#. type: TP
#: uname.1:16
#, no-wrap
msgid "B<-s>, B<--kernel-name>"
msgstr "B<-s>, B<--kernel-name>"

#. type: Plain text
#: uname.1:19
msgid "print the kernel name"
msgstr "打印内核名称"

#. type: TP
#: uname.1:19
#, no-wrap
msgid "B<-n>, B<--nodename>"
msgstr "B<-n>, B<--nodename>"

#. type: Plain text
#: uname.1:22
msgid "print the network node hostname"
msgstr "打印网络节点主机名"

#. type: TP
#: uname.1:22
#, no-wrap
msgid "B<-r>, B<--kernel-release>"
msgstr "B<-r>, B<--kernel-release>"

#. type: Plain text
#: uname.1:25
msgid "print the kernel release"
msgstr "打印内核发行版本号"

#. type: TP
#: uname.1:25
#, no-wrap
msgid "B<-v>, B<--kernel-version>"
msgstr "B<-v>, B<--kernel-version>"

#. type: Plain text
#: uname.1:28
msgid "print the kernel version"
msgstr "打印内核版本号"

#. type: TP
#: uname.1:28
#, no-wrap
msgid "B<-m>, B<--machine>"
msgstr "B<-m>, B<--machine>"

#. type: Plain text
#: uname.1:31
msgid "print the machine hardware name"
msgstr "打印机器硬件名"

#. type: TP
#: uname.1:31
#, no-wrap
msgid "B<-p>, B<--processor>"
msgstr "B<-p>, B<--processor>"

#. type: Plain text
#: uname.1:34
msgid "print the processor type or \"unknown\""
msgstr "打印处理器类型或者是\"unknown\""

#. type: TP
#: uname.1:34
#, no-wrap
msgid "B<-i>, B<--hardware-platform>"
msgstr "B<-i>, B<--hardware-platform>"

#. type: Plain text
#: uname.1:37
msgid "print the hardware platform or \"unknown\""
msgstr "打印硬件平台或者是 \"unknown\""

#. type: TP
#: uname.1:37
#, no-wrap
msgid "B<-o>, B<--operating-system>"
msgstr "B<-o>, B<--operating-system>"

#. type: Plain text
#: uname.1:40
msgid "print the operating system"
msgstr "打印操作系统"

#. type: TP
#: uname.1:40
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: uname.1:43
msgid "display this help and exit"
msgstr "显示帮助并退出"

#. type: TP
#: uname.1:43
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: uname.1:46
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: SH
#: uname.1:46
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: uname.1:48
msgid "Written by David MacKenzie."
msgstr "David MacKenzie撰写。"

#. type: SH
#: uname.1:48
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: uname.1:50
msgid "Report uname bugs to bug-coreutils@gnu.org"
msgstr "发送uname的错误信息到bug-coreutils@gun.org"

#. type: Plain text
#: uname.1:52
msgid ""
"GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils主页：E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: uname.1:54
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助：E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: uname.1:56
msgid ""
"Report uname translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送uname的翻译错误到E<lt>http://github.com/LCTT/man-pages/E<gt>"

#. type: SH
#: uname.1:56
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权"

#. type: Plain text
#: uname.1:59
msgid ""
"Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr ""
"Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL "
"version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
#: uname.1:62
msgid ""
"This is free software: you are free to change and redistribute it.  There is "
"NO WARRANTY, to the extent permitted by law."
msgstr ""
"这是一个自由软件： 你可以自由修改和重新发布它。 在法律允许的范围内， 不提供任"
"何担保。"

#. type: SH
#: uname.1:62
#, no-wrap
msgid "SEE ALSO"
msgstr "请参阅"

#. type: Plain text
#: uname.1:64
msgid "arch(1), uname(2)"
msgstr "arch(1), uname(2)"

#. type: Plain text
#: uname.1:72
msgid ""
"The full documentation for B<uname> is maintained as a Texinfo manual.  If "
"the B<info> and B<uname> programs are properly installed at your site, the "
"command"
msgstr ""
"B<uname> 的完整文档使用 Texinfo 手册进行维护。如果 B<info> 和 B<uname> 程序已"
"经正确地安装在了你的电脑上， 通过下述命令"

#. type: Plain text
#: uname.1:74
msgid "B<info coreutils \\(aquname invocation\\(aq>"
msgstr "B<info coreutils \\(aquname invocation\\(aq>"

#. type: Plain text
#: uname.1:76
msgid "should give you access to the complete manual."
msgstr "可以访问完整的使用手册。"

#. type: SH
#: uname.1:76
#, no-wrap
msgid "翻译信息"
msgstr "翻译信息"

#. type: SS
#: uname.1:77
#, no-wrap
msgid "翻译维护人"
msgstr "翻译维护人"

#. type: Plain text
#: uname.1:79
msgid "译者："
msgstr "译者："

#. type: Plain text
#: uname.1:81
