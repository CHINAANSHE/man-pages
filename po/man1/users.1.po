#. type: TH
#: users.1:2
#, no-wrap
msgid "USERS"
msgstr "USERS"

#. type: TH
#: users.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: users.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: users.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: users.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: users.1:5
msgid ""
"users - print the user names of users currently logged in to the current host"
msgstr "users - 打印登录到当前主机的用户的用户名。"

#. type: SH
#: users.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: users.1:8
msgid "B<users> [I<OPTION>]... [I<FILE>]"
msgstr "B<users> [I<选项>]... [I<文件>]"

#. type: SH
#: users.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: users.1:13
msgid ""
"Output who is currently logged in according to FILE.  If FILE is not "
"specified, use /var/run/utmp.  /var/log/wtmp as FILE is common."
msgstr ""
"根据指定的文件输出当前登录的用户。 如果没有指定文件，默认使用文件 /var/run/"
"utmp。 /var/log/wtmp 也是常用的文件。"

#. type: TP
#: users.1:13
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: users.1:16
msgid "display this help and exit"
msgstr "显示帮助然后退出"

#. type: TP
#: users.1:16
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: users.1:19
msgid "output version information and exit"
msgstr "输出版本信息然后退出"

#. type: SH
#: users.1:19
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: users.1:21
msgid "Written by Joseph Arceneaux and David MacKenzie."
msgstr "由  Joseph Arceneaux 和 David MacKenzie 撰写。"

#. type: SH
#: users.1:21
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: users.1:23
msgid "Report users bugs to bug-coreutils@gnu.org"
msgstr "发送 users 的错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: users.1:25
msgid ""
"GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: users.1:27
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: users.1:29
msgid ""
"Report users translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送 users 的翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

