#. type: SH
#: mv.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: mv.1:5
msgid "mv - move (rename) files"
msgstr ""

#. type: SH
#: mv.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: mv.1:8
msgid "B<mv> [I<OPTION>]... [I<-T>] I<SOURCE DEST>"
msgstr ""

#. type: Plain text
#: mv.1:11
msgid "B<mv> [I<OPTION>]... I<SOURCE>... I<DIRECTORY>"
msgstr ""

#. type: Plain text
#: mv.1:14
msgid "B<mv> [I<OPTION>]... I<-t DIRECTORY SOURCE>..."
msgstr ""

#. type: SH
#: mv.1:14
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: mv.1:18
msgid "Rename SOURCE to DEST, or move SOURCE(s) to DIRECTORY."
msgstr ""

#. type: Plain text
#: mv.1:20
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: mv.1:20
#, no-wrap
msgid "B<--backup>[=I<CONTROL>]"
msgstr ""

#. type: Plain text
#: mv.1:23
msgid "make a backup of each existing destination file"
msgstr ""

#. type: TP
#: mv.1:23
#, no-wrap
msgid "B<-b>"
msgstr ""

#. type: Plain text
#: mv.1:26
msgid "like B<--backup> but does not accept an argument"
msgstr ""

#. type: TP
#: mv.1:26
#, no-wrap
msgid "B<-f>, B<--force>"
msgstr ""

#. type: Plain text
#: mv.1:29
msgid "do not prompt before overwriting"
msgstr ""

#. type: TP
#: mv.1:29
#, no-wrap
msgid "B<-i>, B<--interactive>"
msgstr ""

#. type: Plain text
#: mv.1:32
msgid "prompt before overwrite"
msgstr ""

#. type: TP
#: mv.1:32
#, no-wrap
msgid "B<-n>, B<--no-clobber>"
msgstr ""

#. type: Plain text
#: mv.1:35
msgid "do not overwrite an existing file"
msgstr ""

#. type: Plain text
#: mv.1:37
msgid ""
"If you specify more than one of B<-i>, B<-f>, B<-n>, only the final one "
"takes effect."
msgstr ""

#. type: TP
#: mv.1:37
#, no-wrap
msgid "B<--strip-trailing-slashes>"
msgstr ""

#. type: Plain text
#: mv.1:41
msgid "remove any trailing slashes from each SOURCE argument"
msgstr ""

#. type: TP
#: mv.1:41
#, no-wrap
msgid "B<-S>, B<--suffix>=I<SUFFIX>"
msgstr ""

#. type: Plain text
#: mv.1:44
msgid "override the usual backup suffix"
msgstr ""

#. type: TP
#: mv.1:44
#, no-wrap
msgid "B<-t>, B<--target-directory>=I<DIRECTORY>"
msgstr ""

#. type: Plain text
#: mv.1:47
msgid "move all SOURCE arguments into DIRECTORY"
msgstr ""

#. type: TP
#: mv.1:47
#, no-wrap
msgid "B<-T>, B<--no-target-directory>"
msgstr ""

#. type: Plain text
#: mv.1:50
msgid "treat DEST as a normal file"
msgstr ""

#. type: TP
#: mv.1:50
#, no-wrap
msgid "B<-u>, B<--update>"
msgstr ""

#. type: Plain text
#: mv.1:55
msgid ""
"move only when the SOURCE file is newer than the destination file or when "
"the destination file is missing"
msgstr ""

#. type: TP
#: mv.1:55
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr ""

#. type: Plain text
#: mv.1:58
msgid "explain what is being done"
msgstr ""

#. type: TP
#: mv.1:58
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: mv.1:61
msgid "display this help and exit"
msgstr ""

#. type: TP
#: mv.1:61
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: mv.1:64
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: mv.1:68
msgid ""
"The backup suffix is '~', unless set with B<--suffix> or "
"SIMPLE_BACKUP_SUFFIX.  The version control method may be selected via the "
"B<--backup> option or through the VERSION_CONTROL environment variable.  "
"Here are the values:"
msgstr ""

#. type: TP
#: mv.1:68
#, no-wrap
msgid "none, off"
msgstr ""

#. type: Plain text
#: mv.1:71
msgid "never make backups (even if B<--backup> is given)"
msgstr ""

#. type: TP
#: mv.1:71
#, no-wrap
msgid "numbered, t"
msgstr ""

#. type: Plain text
#: mv.1:74
msgid "make numbered backups"
msgstr ""

#. type: TP
#: mv.1:74
#, no-wrap
msgid "existing, nil"
msgstr ""

#. type: Plain text
#: mv.1:77
msgid "numbered if numbered backups exist, simple otherwise"
msgstr ""

#. type: TP
#: mv.1:77
#, no-wrap
msgid "simple, never"
msgstr ""

#. type: Plain text
#: mv.1:80
msgid "always make simple backups"
msgstr ""

