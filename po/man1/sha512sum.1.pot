#. type: TH
#: sha512sum.1:2
#, no-wrap
msgid "SHA512SUM"
msgstr ""

#. type: TH
#: sha512sum.1:2
#, no-wrap
msgid "October 2013"
msgstr ""

#. type: TH
#: sha512sum.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr ""

#. type: TH
#: sha512sum.1:2
#, no-wrap
msgid "User Commands"
msgstr ""

#. type: SH
#: sha512sum.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: sha512sum.1:5
msgid "sha512sum - compute and check SHA512 message digest"
msgstr ""

#. type: SH
#: sha512sum.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: sha512sum.1:8
msgid "B<sha512sum> [I<OPTION>]... [I<FILE>]..."
msgstr ""

#. type: SH
#: sha512sum.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: sha512sum.1:13
msgid ""
"Print or check SHA512 (512-bit) checksums.  With no FILE, or when FILE is -, "
"read standard input."
msgstr ""

#. type: TP
#: sha512sum.1:13
#, no-wrap
msgid "B<-b>, B<--binary>"
msgstr ""

#. type: Plain text
#: sha512sum.1:16
msgid "read in binary mode"
msgstr ""

#. type: TP
#: sha512sum.1:16
#, no-wrap
msgid "B<-c>, B<--check>"
msgstr ""

#. type: Plain text
#: sha512sum.1:19
msgid "read SHA512 sums from the FILEs and check them"
msgstr ""

#. type: TP
#: sha512sum.1:19
#, no-wrap
msgid "B<--tag>"
msgstr ""

#. type: Plain text
#: sha512sum.1:22
msgid "create a BSD-style checksum"
msgstr ""

#. type: TP
#: sha512sum.1:22
#, no-wrap
msgid "B<-t>, B<--text>"
msgstr ""

#. type: Plain text
#: sha512sum.1:25
msgid "read in text mode (default)"
msgstr ""

#. type: SS
#: sha512sum.1:25
#, no-wrap
msgid "The following three options are useful only when verifying checksums:"
msgstr ""

#. type: TP
#: sha512sum.1:26
#, no-wrap
msgid "B<--quiet>"
msgstr ""

#. type: Plain text
#: sha512sum.1:29
msgid "don't print OK for each successfully verified file"
msgstr ""

#. type: TP
#: sha512sum.1:29
#, no-wrap
msgid "B<--status>"
msgstr ""

#. type: Plain text
#: sha512sum.1:32
msgid "don't output anything, status code shows success"
msgstr ""

#. type: TP
#: sha512sum.1:32
#, no-wrap
msgid "B<-w>, B<--warn>"
msgstr ""

#. type: Plain text
#: sha512sum.1:35
msgid "warn about improperly formatted checksum lines"
msgstr ""

#. type: TP
#: sha512sum.1:35
#, no-wrap
msgid "B<--strict>"
msgstr ""

#. type: Plain text
#: sha512sum.1:38
msgid "with B<--check>, exit non-zero for any invalid input"
msgstr ""

#. type: TP
#: sha512sum.1:38
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: sha512sum.1:41
msgid "display this help and exit"
msgstr ""

#. type: TP
#: sha512sum.1:41
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: sha512sum.1:44
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: sha512sum.1:49
msgid ""
"The sums are computed as described in FIPS-180-2.  When checking, the input "
"should be a former output of this program.  The default mode is to print a "
"line with checksum, a character indicating input mode ('*' for binary, space "
"for text), and name for each FILE."
msgstr ""

