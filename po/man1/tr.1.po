#. type: SH
#: tr.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: tr.1:5
msgid "tr - translate or delete characters"
msgstr "tr    -    转换或删除字符"

#. type: SH
#: tr.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: tr.1:8
msgid "B<tr> [I<OPTION>]... I<SET1 >[I<SET2>]"
msgstr "B<tr> [I<选项>]... I<字符集1 >[I<字符集2>]"

#. type: SH
#: tr.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: tr.1:13
msgid "Translate, squeeze, and/or delete characters from standard input, writing to standard output."
msgstr "转换，合并，删除来自标准流的字符，并写入到输出流中。"

#. type: TP
#: tr.1:13
#, no-wrap
msgid "B<-c>, B<-C>, B<--complement>"
msgstr "B<-c>, B<-C>, B<--complement>"

#. type: Plain text
#: tr.1:16
msgid "use the complement of SET1"
msgstr " 用字符集1的补集    (注意换行符)"

#. type: TP
#: tr.1:16
#, no-wrap
msgid "B<-d>, B<--delete>"
msgstr "B<-d>, B<--delete>"

#. type: Plain text
#: tr.1:19
msgid "delete characters in SET1, do not translate"
msgstr " 删除匹配字符集1的字符，不需要添加字符集2"

#. type: TP
#: tr.1:19
#, no-wrap
msgid "B<-s>, B<--squeeze-repeats>"
msgstr "B<-s>, B<--squeeze-repeats>"

#. type: Plain text
#: tr.1:24
msgid "replace each input sequence of a repeated character that is listed in SET1 with a single occurrence of that character"
msgstr "合并字符集2替换字符集1后重复的字符"

#. type: TP
#: tr.1:24
#, no-wrap
msgid "B<-t>, B<--truncate-set1>"
msgstr "B<-t>, B<--truncate-set1>"

#. type: Plain text
#: tr.1:27
msgid "first truncate SET1 to length of SET2"
msgstr "截断字符集1的长度与字符集2一致"

#. type: TP
#: tr.1:27
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: tr.1:30
msgid "display this help and exit"
msgstr "显示帮助并退出"

#. type: TP
#: tr.1:30
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: tr.1:33
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: Plain text
#: tr.1:36
msgid "SETs are specified as strings of characters.  Most represent themselves.  Interpreted sequences are:"
msgstr "集合可以指定为字符串.更能代表它们的解释型字符码有 :"

#. type: TP
#: tr.1:36
#, no-wrap
msgid "\\eNNN"
msgstr "\\eNNN"

#. type: Plain text
#: tr.1:39
msgid "character with octal value NNN (1 to 3 octal digits)"
msgstr "用NNN（1到3个八进制数）来表示字符"

#. type: TP
#: tr.1:39
#, no-wrap
msgid "\\e\\e"
msgstr "\\e\\e"

#. type: Plain text
#: tr.1:42
msgid "backslash"
msgstr "反斜杠"

#. type: TP
#: tr.1:42
#, no-wrap
msgid "\\ea"
msgstr "\\ea"

