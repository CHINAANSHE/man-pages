msgstr ""

#. type: Plain text
#: chroot.1:5
msgid "chroot - run command or interactive shell with special root directory"
msgstr ""

#. type: SH
#: chroot.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: chroot.1:8
msgid "B<chroot> [I<OPTION>] I<NEWROOT >[I<COMMAND >[I<ARG>]...]"
msgstr ""

#. type: Plain text
#: chroot.1:11
msgid "B<chroot> I<OPTION>"
msgstr ""

#. type: SH
#: chroot.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: chroot.1:15
msgid "Run COMMAND with root directory set to NEWROOT."
msgstr ""

#. type: TP
#: chroot.1:15
#, no-wrap
msgid "B<--userspec>=I<USER>:GROUP"
msgstr ""

#. type: Plain text
#: chroot.1:18
msgid "specify user and group (ID or name) to use"
msgstr ""

#. type: TP
#: chroot.1:18
#, no-wrap
msgid "B<--groups>=I<G_LIST>"
msgstr ""

#. type: Plain text
#: chroot.1:21
msgid "specify supplementary groups as g1,g2,..,gN"
msgstr ""

#. type: TP
#: chroot.1:21
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: chroot.1:24
msgid "display this help and exit"
msgstr ""

#. type: TP
#: chroot.1:24
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: chroot.1:27
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: chroot.1:29
msgid "If no command is given, run '${SHELL} B<-i>' (default: '/bin/sh B<-i>')."
msgstr ""

