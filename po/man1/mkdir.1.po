#. type: TH
#: mkdir.1:2
#, no-wrap
msgid "MKDIR"
msgstr "MKDIR"

#. type: TH
#: mkdir.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: mkdir.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: mkdir.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: mkdir.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: mkdir.1:5
msgid "mkdir - make directories"
msgstr "mkdir-创建目录"

#. type: SH
#: mkdir.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "简介"

#. type: Plain text
#: mkdir.1:8
msgid "B<mkdir> [I<OPTION>]... I<DIRECTORY>..."
msgstr "B<mkdir>[I<选项>]...I<目录>"

#. type: SH
#: mkdir.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "说明"

#. type: Plain text
#: mkdir.1:12
msgid "Create the DIRECTORY(ies), if they do not already exist."
msgstr "如果目录不存在则创建目录"

#. type: Plain text
#: mkdir.1:14
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr "对于长选项来说必须的参数,对于短选项也是必须的。"

#. type: TP
#: mkdir.1:14
#, no-wrap
msgid "B<-m>, B<--mode>=I<MODE>"
msgstr "B<-m>, B<--mode>=I<模式>"

#. type: Plain text
#: mkdir.1:17
msgid "set file mode (as in chmod), not a=rwx - umask"
msgstr "设置文件模式（就像chmod命令一样），而不是使用 a=rwx -umask"

#. type: TP
#: mkdir.1:17
#, no-wrap
msgid "B<-p>, B<--parents>"
msgstr "B<-p>, B<--parents>"

#. type: Plain text
#: mkdir.1:20
msgid "no error if existing, make parent directories as needed"
msgstr "建立需要的父目录，如果存在不报错"

#. type: TP
#: mkdir.1:20
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr "B<-v>, B<--verbose>"

#. type: Plain text
#: mkdir.1:23
msgid "print a message for each created directory"
msgstr "打印出新建的每一个目录名"

#. type: TP
#: mkdir.1:23
#, no-wrap
msgid "B<-Z>, B<--context>=I<CTX>"
msgstr "B<-Z>, B<--context>=I<CTX>"

#. type: Plain text
#: mkdir.1:27
msgid "set the SELinux security context of each created directory to CTX"
msgstr "为每一个创建的目录设置SELinux安全上下文为CTX"

#. type: TP
#: mkdir.1:27
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: mkdir.1:30
msgid "display this help and exit"
msgstr "显示帮助并退出"

#. type: TP
#: mkdir.1:30
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: mkdir.1:33
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: SH
#: mkdir.1:33
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: mkdir.1:35
msgid "Written by David MacKenzie."
msgstr "由David MacKenzie撰写"

#. type: SH
#: mkdir.1:35
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: mkdir.1:37
msgid "Report mkdir bugs to bug-coreutils@gnu.org"
msgstr "向bug-coreutils@gnu.org报告mkdir的错误"

#. type: Plain text
#: mkdir.1:39
msgid "GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页：  E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: mkdir.1:41
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: mkdir.1:43
msgid "Report mkdir translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

#. type: SH
#: mkdir.1:43
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权"

#. type: Plain text
#: mkdir.1:46
msgid "Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr "Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
#: mkdir.1:49
msgid "This is free software: you are free to change and redistribute it.  There is NO WARRANTY, to the extent permitted by law."
msgstr "这是一个自由软件： 你可以自由修改和重新发布它。 在法律允许的范围内， 不提供任何担保"

#. type: SH
#: mkdir.1:49
#, no-wrap
msgid "SEE ALSO"
msgstr "请参阅"

#. type: Plain text
#: mkdir.1:51
msgid "mkdir(2)"
msgstr "mkdir(2)"

#. type: Plain text
#: mkdir.1:59
msgid "The full documentation for B<mkdir> is maintained as a Texinfo manual.  If the B<info> and B<mkdir> programs are properly installed at your site, the command"
msgstr "B<mkdir> 的完整文档使用 Texinfo 手册进行维护。如果 B<info> 和 B<mkdir> 程序已经正确地安装在了你的电脑上， 通过下述命令"

#. type: Plain text
#: mkdir.1:61
msgid "B<info coreutils \\(aqmkdir invocation\\(aq>"
msgstr "B<info coreutils \\(aqmkdir invocation\\(aq>"

#. type: Plain text
#: mkdir.1:63
msgid "should give you access to the complete manual."
msgstr "可以访问完整的使用手册"

