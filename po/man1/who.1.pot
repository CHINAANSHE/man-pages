#. type: Plain text
#: who.1:5
msgid "who - show who is logged on"
msgstr ""

#. type: SH
#: who.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: who.1:8
msgid "B<who> [I<OPTION>]... [ I<FILE | ARG1 ARG2 >]"
msgstr ""

#. type: SH
#: who.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: who.1:12
msgid "Print information about users who are currently logged in."
msgstr ""

#. type: TP
#: who.1:12
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr ""

#. type: Plain text
#: who.1:15
msgid "same as B<-b> B<-d> B<--login> B<-p> B<-r> B<-t> B<-T> B<-u>"
msgstr ""

#. type: TP
#: who.1:15
#, no-wrap
msgid "B<-b>, B<--boot>"
msgstr ""

#. type: Plain text
#: who.1:18
msgid "time of last system boot"
msgstr ""

#. type: TP
#: who.1:18
#, no-wrap
msgid "B<-d>, B<--dead>"
msgstr ""

#. type: Plain text
#: who.1:21
msgid "print dead processes"
msgstr ""

#. type: TP
#: who.1:21
#, no-wrap
msgid "B<-H>, B<--heading>"
msgstr ""

#. type: Plain text
#: who.1:24
msgid "print line of column headings"
msgstr ""

#. type: TP
#: who.1:24
#, no-wrap
msgid "B<-l>, B<--login>"
msgstr ""

#. type: Plain text
#: who.1:27
msgid "print system login processes"
msgstr ""

#. type: TP
#: who.1:27
#, no-wrap
msgid "B<--lookup>"
msgstr ""

#. type: Plain text
#: who.1:30
msgid "attempt to canonicalize hostnames via DNS"
msgstr ""

#. type: TP
#: who.1:30
#, no-wrap
msgid "B<-m>"
msgstr ""

#. type: Plain text
#: who.1:33
msgid "only hostname and user associated with stdin"
msgstr ""

#. type: TP
#: who.1:33
#, no-wrap
msgid "B<-p>, B<--process>"
msgstr ""

#. type: Plain text
#: who.1:36
msgid "print active processes spawned by init"
msgstr ""

#. type: TP
#: who.1:36
#, no-wrap
msgid "B<-q>, B<--count>"
msgstr ""

#. type: Plain text
#: who.1:39
msgid "all login names and number of users logged on"
msgstr ""

#. type: TP
#: who.1:39
#, no-wrap
msgid "B<-r>, B<--runlevel>"
msgstr ""

#. type: Plain text
#: who.1:42
msgid "print current runlevel"
msgstr ""

#. type: TP
#: who.1:42
#, no-wrap
msgid "B<-s>, B<--short>"
msgstr ""

#. type: Plain text
#: who.1:45
msgid "print only name, line, and time (default)"
msgstr ""

#. type: TP
#: who.1:45
#, no-wrap
msgid "B<-t>, B<--time>"
msgstr ""

#. type: Plain text
#: who.1:48
msgid "print last system clock change"
msgstr ""

#. type: TP
#: who.1:48
#, no-wrap
msgid "B<-T>, B<-w>, B<--mesg>"
msgstr ""

#. type: Plain text
#: who.1:51
msgid "add user's message status as +, - or ?"
msgstr ""

#. type: TP
#: who.1:51
#, no-wrap
msgid "B<-u>, B<--users>"
msgstr ""

#. type: Plain text
#: who.1:54
msgid "list users logged in"
msgstr ""

#. type: TP
#: who.1:54
#, no-wrap
msgid "B<--message>"
msgstr ""

#. type: Plain text
#: who.1:57 who.1:60
msgid "same as B<-T>"
msgstr ""

#. type: TP
#: who.1:57
#, no-wrap
msgid "B<--writable>"
msgstr ""

#. type: TP
#: who.1:60
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: who.1:63
msgid "display this help and exit"
msgstr ""

#. type: TP
#: who.1:63
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: who.1:66
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: who.1:69
msgid ""
"If FILE is not specified, use /var/run/utmp.  /var/log/wtmp as FILE is "
"common.  If ARG1 ARG2 given, B<-m> presumed: 'am i' or 'mom likes' are usual."
msgstr ""

