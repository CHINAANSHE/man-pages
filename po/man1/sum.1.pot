#: sum.1:12
msgid "Print checksum and block counts for each FILE."
msgstr ""

#. type: TP
#: sum.1:12
#, no-wrap
msgid "B<-r>"
msgstr ""

#. type: Plain text
#: sum.1:15
msgid "use BSD sum algorithm, use 1K blocks"
msgstr ""

#. type: TP
#: sum.1:15
#, no-wrap
msgid "B<-s>, B<--sysv>"
msgstr ""

#. type: Plain text
#: sum.1:18
msgid "use System V sum algorithm, use 512 bytes blocks"
msgstr ""

#. type: TP
#: sum.1:18
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: sum.1:21
msgid "display this help and exit"
msgstr ""

