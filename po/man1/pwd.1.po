#. type: TH
#: pwd.1:2
#, no-wrap
msgid "PWD"
msgstr "PWD"

#. type: TH
#: pwd.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: pwd.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: pwd.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: pwd.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: pwd.1:5
msgid "pwd - print name of current/working directory"
msgstr "pwd - 显示当前目录或者工作目录"

#. type: SH
#: pwd.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: pwd.1:8
msgid "B<pwd> [I<OPTION>]..."
msgstr "B<pwd> [I<选项>]…"

#. type: SH
#: pwd.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: pwd.1:12
msgid "Print the full filename of the current working directory."
msgstr "打印当前工作目录的完整文件名"

#. type: TP
#: pwd.1:12
#, no-wrap
msgid "B<-L>, B<--logical>"
msgstr "B<-L>, B<--logical>"

#. type: Plain text
#: pwd.1:15
msgid "use PWD from environment, even if it contains symlinks"
msgstr "显示环境变量PWD的值，甚至它包含符号链接"

#. type: TP
#: pwd.1:15
#, no-wrap
msgid "B<-P>, B<--physical>"
msgstr "B<-P>, B<--physical>"

#. type: Plain text
#: pwd.1:18
msgid "avoid all symlinks"
msgstr "忽略全部的符号链接"

#. type: TP
#: pwd.1:18
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: pwd.1:21
msgid "display this help and exit"
msgstr "显示帮助并退出"

#. type: TP
#: pwd.1:21
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: pwd.1:24
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: Plain text
#: pwd.1:28
msgid "NOTE: your shell may have its own version of pwd, which usually supersedes the version described here.  Please refer to your shell's documentation for details about the options it supports."
msgstr "注意： 你的 shell 也许有自己版本的 pwd 命令， 他们通常会取代这里描述的版本。 请参考你的 shell 提供的文档来了解它所支持的选项的细节。"

#. type: SH
#: pwd.1:28
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: pwd.1:30
msgid "Written by Jim Meyering."
msgstr "由 Jim Meyering 撰写"

#. type: SH
#: pwd.1:30
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: pwd.1:32
msgid "Report pwd bugs to bug-coreutils@gnu.org"
msgstr "发送错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: pwd.1:34
msgid "GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页：  E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: pwd.1:36
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: pwd.1:38
msgid "Report pwd translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

#. type: SH
#: pwd.1:38
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权"

#. type: Plain text
#: pwd.1:41
msgid "Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr "Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
#: pwd.1:44
msgid "This is free software: you are free to change and redistribute it.  There is NO WARRANTY, to the extent permitted by law."
msgstr "这是一个自由软件： 你可以自由修改和重新发布它。 在法律允许的范围内， 不提供任何担保"

#. type: SH
#: pwd.1:44
#, no-wrap
msgid "SEE ALSO"
msgstr "请参阅"

#. type: Plain text
#: pwd.1:46
msgid "getcwd(3)"
msgstr "getcwd(3)"

#. type: Plain text
#: pwd.1:54
msgid "The full documentation for B<pwd> is maintained as a Texinfo manual.  If the B<info> and B<pwd> programs are properly installed at your site, the command"
msgstr "B<pwd> 的完整文档使用 Texinfo 手册进行维护。如果 B<info> 和 B<pwd> 程序已经正确地安装在了你的电脑上， 通过下述命令"

#. type: Plain text
#: pwd.1:56
msgid "B<info coreutils \\(aqpwd invocation\\(aq>"
msgstr "B<info coreutils \\(aqpwd invocation\\(aq>"

#. type: Plain text
#: pwd.1:58
msgid "should give you access to the complete manual."
msgstr "可以访问完整的使用手册"

