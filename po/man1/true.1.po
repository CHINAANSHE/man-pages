#. type: TH
#: true.1:2
#, no-wrap
msgid "TRUE"
msgstr "TURE"

#. type: TH
#: true.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: true.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: true.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: true.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: true.1:5
msgid "true - do nothing, successfully"
msgstr "true - 正确的不做任何事"

#. type: SH
#: true.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: true.1:8
msgid "B<true> [I<ignored command line arguments>]"
msgstr "B<true> [I<忽略 命令行 参数>]"

#. type: Plain text
#: true.1:11
msgid "B<true> I<OPTION>"
msgstr "B<true> I<选项>"

#. type: SH
#: true.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: true.1:15
msgid "Exit with a status code indicating success."
msgstr "退出，同时返回成功状态码。"

#. type: TP
#: true.1:15
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: true.1:18
msgid "display this help and exit"
msgstr "显示帮助信息并退出"

#. type: TP
#: true.1:18
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: true.1:21
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: Plain text
#: true.1:25
msgid "NOTE: your shell may have its own version of true, which usually supersedes the version described here.  Please refer to your shell's documentation for details about the options it supports."
msgstr "注解：你的 shell 可能有自己的 true 版本，通常会取代这里描述的版本。请参阅您的 shell 文档以便详细了解它支持的选项。"

#. type: SH
#: true.1:25
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: true.1:27
msgid "Written by Jim Meyering."
msgstr "由Jim Meyering 撰写。 "

#. type: SH
#: true.1:27
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: true.1:29
msgid "Report true bugs to bug-coreutils@gnu.org"
msgstr "发送错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: true.1:31
msgid "GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: true.1:33
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: true.1:35
msgid "Report true translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

#. type: SH
#: true.1:35
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权声明"

#. type: Plain text
#: true.1:38
msgid "Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr "Copyright \\(co 2013 Free Software Foundation, Inc. License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
#: true.1:41
msgid "This is free software: you are free to change and redistribute it.  There is NO WARRANTY, to the extent permitted by law."
msgstr "这是一个自由软件： 你可以自由修改和重新发布它。 在法律允许的范围内， 不提供任何担保"

#. type: SH
#: true.1:41
#, no-wrap
msgid "SEE ALSO"
msgstr "请参阅"

#. type: Plain text
#: true.1:49
msgid "The full documentation for B<true> is maintained as a Texinfo manual.  If the B<info> and B<true> programs are properly installed at your site, the command"
msgstr "B<true>的完整文档使用 Texinfo 手册进行维护。如果 B<info> 和 B<true> 程序已经正确地安装在了你的电脑上，通过下述命令"

#. type: Plain text
#: true.1:51
msgid "B<info coreutils \\(aqtrue invocation\\(aq>"
msgstr "B<info coreutils \\(aqtrue invocation\\(aq>"

#. type: Plain text
#: true.1:53
msgid "should give you access to the complete manual."
msgstr "可以访问完整的使用手册"

