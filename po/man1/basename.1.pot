#. type: SH
#: basename.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: basename.1:5
msgid "basename - strip directory and suffix from filenames"
msgstr ""

#. type: SH
#: basename.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: basename.1:8
msgid "B<basename> I<NAME >[I<SUFFIX>]"
msgstr ""

#. type: Plain text
#: basename.1:11
msgid "B<basename> I<OPTION>... I<NAME>..."
msgstr ""

#. type: SH
#: basename.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: basename.1:16
msgid ""
"Print NAME with any leading directory components removed.  If specified, "
"also remove a trailing SUFFIX."
msgstr ""

#. type: Plain text
#: basename.1:18
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: basename.1:18
#, no-wrap
msgid "B<-a>, B<--multiple>"
msgstr ""

#. type: Plain text
#: basename.1:21
msgid "support multiple arguments and treat each as a NAME"
msgstr ""

#. type: TP
#: basename.1:21
#, no-wrap
msgid "B<-s>, B<--suffix>=I<SUFFIX>"
msgstr ""

#. type: Plain text
#: basename.1:24
msgid "remove a trailing SUFFIX"
msgstr ""

#. type: TP
#: basename.1:24
#, no-wrap
msgid "B<-z>, B<--zero>"
msgstr ""

#. type: Plain text
#: basename.1:27
msgid "separate output with NUL rather than newline"
msgstr ""

#. type: TP
#: basename.1:27
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: basename.1:30
msgid "display this help and exit"
msgstr ""

#. type: TP
#: basename.1:30
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: basename.1:33
msgid "output version information and exit"
msgstr ""

#. type: SH
#: basename.1:33
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: TP
#: basename.1:34
#, no-wrap
msgid "basename /usr/bin/sort"
msgstr ""

#. type: Plain text
#: basename.1:37
msgid "-E<gt> \"sort\""
msgstr ""

#. type: TP
#: basename.1:37
#, no-wrap
msgid "basename include/stdio.h .h"
msgstr ""

#. type: Plain text
#: basename.1:40 basename.1:43
msgid "-E<gt> \"stdio\""
msgstr ""

#. type: TP
#: basename.1:40
#, no-wrap
msgid "basename B<-s> .h include/stdio.h"
msgstr ""

#. type: TP
#: basename.1:43
#, no-wrap
msgid "basename B<-a> any/str1 any/str2"
msgstr ""

#. type: Plain text
#: basename.1:46
msgid "-E<gt> \"str1\" followed by \"str2\""
msgstr ""

