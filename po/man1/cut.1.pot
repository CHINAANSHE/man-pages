#, no-wrap
msgid "CUT"
msgstr ""

#. type: TH
#: cut.1:2
#, no-wrap
msgid "October 2013"
msgstr ""

#. type: TH
#: cut.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr ""

#. type: TH
#: cut.1:2
#, no-wrap
msgid "User Commands"
msgstr ""

#. type: SH
#: cut.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: cut.1:5
msgid "cut - remove sections from each line of files"
msgstr ""

#. type: SH
#: cut.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: cut.1:8
msgid "B<cut> I<OPTION>... [I<FILE>]..."
msgstr ""

#. type: SH
#: cut.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: cut.1:12
msgid "Print selected parts of lines from each FILE to standard output."
msgstr ""

#. type: Plain text
#: cut.1:14
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: cut.1:14
#, no-wrap
msgid "B<-b>, B<--bytes>=I<LIST>"
msgstr ""

#. type: Plain text
#: cut.1:17
msgid "select only these bytes"
msgstr ""

#. type: TP
#: cut.1:17
#, no-wrap
msgid "B<-c>, B<--characters>=I<LIST>"
msgstr ""

#. type: Plain text
#: cut.1:20
msgid "select only these characters"
msgstr ""

#. type: TP
#: cut.1:20
#, no-wrap
msgid "B<-d>, B<--delimiter>=I<DELIM>"
msgstr ""

#. type: Plain text
#: cut.1:23
msgid "use DELIM instead of TAB for field delimiter"
msgstr ""

#. type: TP
#: cut.1:23
#, no-wrap
msgid "B<-f>, B<--fields>=I<LIST>"
msgstr ""

#. type: Plain text
#: cut.1:28
msgid ""
"select only these fields; also print any line that contains no delimiter "
"character, unless the B<-s> option is specified"
msgstr ""

#. type: TP
#: cut.1:28
#, no-wrap
msgid "B<-n>"
msgstr ""

#. type: Plain text
#: cut.1:31
msgid "(ignored)"
msgstr ""

#. type: TP
#: cut.1:31
#, no-wrap
msgid "B<--complement>"
msgstr ""

#. type: Plain text
#: cut.1:35
msgid "complement the set of selected bytes, characters or fields"
msgstr ""

#. type: TP
#: cut.1:35
#, no-wrap
msgid "B<-s>, B<--only-delimited>"
msgstr ""

#. type: Plain text
#: cut.1:38
msgid "do not print lines not containing delimiters"
msgstr ""

#. type: TP
#: cut.1:38
#, no-wrap
msgid "B<--output-delimiter>=I<STRING>"
msgstr ""

#. type: Plain text
#: cut.1:42
msgid ""
"use STRING as the output delimiter the default is to use the input delimiter"
msgstr ""

#. type: TP
#: cut.1:42
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: cut.1:45
msgid "display this help and exit"
msgstr ""

#. type: TP
#: cut.1:45
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: cut.1:48
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: cut.1:53
msgid ""
"Use one, and only one of B<-b>, B<-c> or B<-f>.  Each LIST is made up of one "
"range, or many ranges separated by commas.  Selected input is written in the "
"same order that it is read, and is written exactly once.  Each range is one "
"of:"
msgstr ""

#. type: TP
#: cut.1:53
#, no-wrap
msgid "N"
msgstr ""

#. type: Plain text
#: cut.1:56
msgid "N'th byte, character or field, counted from 1"
msgstr ""

#. type: TP
#: cut.1:56
#, no-wrap
msgid "N-"
msgstr ""

#. type: Plain text
#: cut.1:59
msgid "from N'th byte, character or field, to end of line"
msgstr ""

#. type: TP
#: cut.1:59
#, no-wrap
msgid "N-M"
msgstr ""

#. type: Plain text
#: cut.1:62
msgid "from N'th to M'th (included) byte, character or field"
msgstr ""

#. type: TP
#: cut.1:62
#, no-wrap
msgid "B<-M>"
msgstr ""

#. type: Plain text
#: cut.1:65
msgid "from first to M'th (included) byte, character or field"
msgstr ""

#. type: Plain text
#: cut.1:67
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

