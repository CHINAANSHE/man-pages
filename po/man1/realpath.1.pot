#. type: SH
#: realpath.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: realpath.1:5
msgid "realpath - print the resolved path"
msgstr ""

#. type: SH
#: realpath.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: realpath.1:8
msgid "B<realpath> [I<OPTION>]... I<FILE>..."
msgstr ""

#. type: SH
#: realpath.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: realpath.1:13
msgid ""
"Print the resolved absolute file name; all but the last component must exist"
msgstr ""

#. type: TP
#: realpath.1:13
#, no-wrap
msgid "B<-e>, B<--canonicalize-existing>"
msgstr ""

#. type: Plain text
#: realpath.1:16
msgid "all components of the path must exist"
msgstr ""

#. type: TP
#: realpath.1:16
#, no-wrap
msgid "B<-m>, B<--canonicalize-missing>"
msgstr ""

#. type: Plain text
#: realpath.1:19
msgid "no components of the path need exist"
msgstr ""

#. type: TP
#: realpath.1:19
#, no-wrap
msgid "B<-L>, B<--logical>"
msgstr ""

#. type: Plain text
#: realpath.1:22
msgid "resolve '..' components before symlinks"
msgstr ""

#. type: TP
#: realpath.1:22
#, no-wrap
msgid "B<-P>, B<--physical>"
msgstr ""

#. type: Plain text
#: realpath.1:25
msgid "resolve symlinks as encountered (default)"
msgstr ""

#. type: TP
#: realpath.1:25
#, no-wrap
msgid "B<-q>, B<--quiet>"
msgstr ""

#. type: Plain text
#: realpath.1:28
msgid "suppress most error messages"
msgstr ""

#. type: TP
#: realpath.1:28
#, no-wrap
msgid "B<--relative-to>=I<FILE>"
msgstr ""

#. type: Plain text
#: realpath.1:31
msgid "print the resolved path relative to FILE"
msgstr ""

#. type: TP
#: realpath.1:31
#, no-wrap
msgid "B<--relative-base>=I<FILE>"
msgstr ""

#. type: Plain text
#: realpath.1:34
msgid "print absolute paths unless paths below FILE"
msgstr ""

#. type: TP
#: realpath.1:34
#, no-wrap
msgid "B<-s>, B<--strip>, B<--no-symlinks>"
msgstr ""

#. type: Plain text
#: realpath.1:37
msgid "don't expand symlinks"
msgstr ""

#. type: TP
#: realpath.1:37
#, no-wrap
msgid "B<-z>, B<--zero>"
msgstr ""

#. type: Plain text
#: realpath.1:40
msgid "separate output with NUL rather than newline"
msgstr ""

#. type: TP
#: realpath.1:40
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: realpath.1:43
msgid "display this help and exit"
msgstr ""

#. type: TP
#: realpath.1:43
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: realpath.1:46
msgid "output version information and exit"
msgstr ""

