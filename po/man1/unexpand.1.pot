#. type: SH
#: unexpand.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: unexpand.1:5
msgid "unexpand - convert spaces to tabs"
msgstr ""

#. type: SH
#: unexpand.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: unexpand.1:8
msgid "B<unexpand> [I<OPTION>]... [I<FILE>]..."
msgstr ""

#. type: SH
#: unexpand.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: unexpand.1:13
msgid ""
"Convert blanks in each FILE to tabs, writing to standard output.  With no "
"FILE, or when FILE is -, read standard input."
msgstr ""

#. type: Plain text
#: unexpand.1:15
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: unexpand.1:15
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr ""

#. type: Plain text
#: unexpand.1:18
msgid "convert all blanks, instead of just initial blanks"
msgstr ""

#. type: TP
#: unexpand.1:18
#, no-wrap
msgid "B<--first-only>"
msgstr ""

#. type: Plain text
#: unexpand.1:21
msgid "convert only leading sequences of blanks (overrides B<-a>)"
msgstr ""

#. type: TP
#: unexpand.1:21
#, no-wrap
msgid "B<-t>, B<--tabs>=I<N>"
msgstr ""

#. type: Plain text
#: unexpand.1:24
msgid "have tabs N characters apart instead of 8 (enables B<-a>)"
msgstr ""

#. type: TP
#: unexpand.1:24
#, no-wrap
msgid "B<-t>, B<--tabs>=I<LIST>"
msgstr ""

#. type: Plain text
#: unexpand.1:27
msgid "use comma separated LIST of tab positions (enables B<-a>)"
msgstr ""

#. type: TP
#: unexpand.1:27
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: unexpand.1:30
msgid "display this help and exit"
msgstr ""

#. type: TP
#: unexpand.1:30
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: unexpand.1:33
msgid "output version information and exit"
msgstr ""

