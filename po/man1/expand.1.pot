#. type: SH
#: expand.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: expand.1:5
msgid "expand - convert tabs to spaces"
msgstr ""

#. type: SH
#: expand.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: expand.1:8
msgid "B<expand> [I<OPTION>]... [I<FILE>]..."
msgstr ""

#. type: SH
#: expand.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: expand.1:13
msgid ""
"Convert tabs in each FILE to spaces, writing to standard output.  With no "
"FILE, or when FILE is -, read standard input."
msgstr ""

#. type: Plain text
#: expand.1:15
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: expand.1:15
#, no-wrap
msgid "B<-i>, B<--initial>"
msgstr ""

#. type: Plain text
#: expand.1:18
msgid "do not convert tabs after non blanks"
msgstr ""

#. type: TP
#: expand.1:18
#, no-wrap
msgid "B<-t>, B<--tabs>=I<NUMBER>"
msgstr ""

#. type: Plain text
#: expand.1:21
msgid "have tabs NUMBER characters apart, not 8"
msgstr ""

#. type: TP
#: expand.1:21
#, no-wrap
msgid "B<-t>, B<--tabs>=I<LIST>"
msgstr ""

#. type: Plain text
#: expand.1:24
msgid "use comma separated list of explicit tab positions"
msgstr ""

#. type: TP
#: expand.1:24
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: expand.1:27
msgid "display this help and exit"
msgstr ""

#. type: TP
#: expand.1:27
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: expand.1:30
msgid "output version information and exit"
msgstr ""

