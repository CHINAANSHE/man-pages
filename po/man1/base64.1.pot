msgstr ""

#. type: TH
#: base64.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr ""

#. type: TH
#: base64.1:2
#, no-wrap
msgid "User Commands"
msgstr ""

#. type: SH
#: base64.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: base64.1:5
msgid "base64 - base64 encode/decode data and print to standard output"
msgstr ""

#. type: SH
#: base64.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: base64.1:8
msgid "B<base64> [I<OPTION>]... [I<FILE>]"
msgstr ""

#. type: SH
#: base64.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: base64.1:12
msgid "Base64 encode or decode FILE, or standard input, to standard output."
msgstr ""

#. type: Plain text
#: base64.1:14
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: base64.1:14
#, no-wrap
msgid "B<-d>, B<--decode>"
msgstr ""

#. type: Plain text
#: base64.1:17
msgid "decode data"
msgstr ""

#. type: TP
#: base64.1:17
#, no-wrap
msgid "B<-i>, B<--ignore-garbage>"
msgstr ""

#. type: Plain text
#: base64.1:20
msgid "when decoding, ignore non-alphabet characters"
msgstr ""

#. type: TP
#: base64.1:20
#, no-wrap
msgid "B<-w>, B<--wrap>=I<COLS>"
msgstr ""

#. type: Plain text
#: base64.1:24
msgid ""
"wrap encoded lines after COLS character (default 76).  Use 0 to disable line "
"wrapping"
msgstr ""

#. type: TP
#: base64.1:24
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: base64.1:27
msgid "display this help and exit"
msgstr ""

#. type: TP
#: base64.1:27
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: base64.1:30
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: base64.1:32
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

#. type: Plain text
#: base64.1:37
msgid ""
"The data are encoded as described for the base64 alphabet in RFC 3548.  When "
"decoding, the input may contain newlines in addition to the bytes of the "
"formal base64 alphabet.  Use B<--ignore-garbage> to attempt to recover from "
"any other non-alphabet bytes in the encoded stream."
msgstr ""

