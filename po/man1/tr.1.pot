#. type: SH
#: tr.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: tr.1:8
msgid "B<tr> [I<OPTION>]... I<SET1 >[I<SET2>]"
msgstr ""

#. type: SH
#: tr.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: tr.1:13
msgid ""
"Translate, squeeze, and/or delete characters from standard input, writing to "
"standard output."
msgstr ""

#. type: TP
#: tr.1:13
#, no-wrap
msgid "B<-c>, B<-C>, B<--complement>"
msgstr ""

#. type: Plain text
#: tr.1:16
msgid "use the complement of SET1"
msgstr ""

#. type: TP
#: tr.1:16
#, no-wrap
msgid "B<-d>, B<--delete>"
msgstr ""

#. type: Plain text
#: tr.1:19
msgid "delete characters in SET1, do not translate"
msgstr ""

#. type: TP
#: tr.1:19
#, no-wrap
msgid "B<-s>, B<--squeeze-repeats>"
msgstr ""

#. type: Plain text
#: tr.1:24
msgid ""
"replace each input sequence of a repeated character that is listed in SET1 "
"with a single occurrence of that character"
msgstr ""

#. type: TP
#: tr.1:24
#, no-wrap
msgid "B<-t>, B<--truncate-set1>"
msgstr ""

#. type: Plain text
#: tr.1:27
msgid "first truncate SET1 to length of SET2"
msgstr ""

