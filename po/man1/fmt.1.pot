#. type: SH
#: fmt.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: fmt.1:5
msgid "fmt - simple optimal text formatter"
msgstr ""

#. type: SH
#: fmt.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: fmt.1:8
msgid "B<fmt> [I<-WIDTH>] [I<OPTION>]... [I<FILE>]..."
msgstr ""

#. type: SH
#: fmt.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: fmt.1:13
msgid ""
"Reformat each paragraph in the FILE(s), writing to standard output.  The "
"option B<-WIDTH> is an abbreviated form of B<--width>=I<DIGITS>."
msgstr ""

#. type: Plain text
#: fmt.1:15
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: fmt.1:15
#, no-wrap
msgid "B<-c>, B<--crown-margin>"
msgstr ""

#. type: Plain text
#: fmt.1:18
msgid "preserve indentation of first two lines"
msgstr ""

#. type: TP
#: fmt.1:18
#, no-wrap
msgid "B<-p>, B<--prefix>=I<STRING>"
msgstr ""

#. type: Plain text
#: fmt.1:22
msgid ""
"reformat only lines beginning with STRING, reattaching the prefix to "
"reformatted lines"
msgstr ""

#. type: TP
#: fmt.1:22
#, no-wrap
msgid "B<-s>, B<--split-only>"
msgstr ""

#. type: Plain text
#: fmt.1:25
msgid "split long lines, but do not refill"
msgstr ""

#. type: TP
#: fmt.1:25
#, no-wrap
msgid "B<-t>, B<--tagged-paragraph>"
msgstr ""

#. type: Plain text
#: fmt.1:28
msgid "indentation of first line different from second"
msgstr ""

#. type: TP
#: fmt.1:28
#, no-wrap
msgid "B<-u>, B<--uniform-spacing>"
msgstr ""

#. type: Plain text
#: fmt.1:31
msgid "one space between words, two after sentences"
msgstr ""

#. type: TP
#: fmt.1:31
#, no-wrap
msgid "B<-w>, B<--width>=I<WIDTH>"
msgstr ""

#. type: Plain text
#: fmt.1:34
msgid "maximum line width (default of 75 columns)"
msgstr ""

#. type: TP
#: fmt.1:34
#, no-wrap
msgid "B<-g>, B<--goal>=I<WIDTH>"
msgstr ""

#. type: Plain text
#: fmt.1:37
msgid "goal width (default of 93% of width)"
msgstr ""

#. type: TP
#: fmt.1:37
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: fmt.1:40
msgid "display this help and exit"
msgstr ""

#. type: TP
#: fmt.1:40
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: fmt.1:43
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: fmt.1:45
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

