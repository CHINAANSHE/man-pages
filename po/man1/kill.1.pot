#. type: SH
#: kill.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: kill.1:5
msgid "kill - send signals to processes, or list signals"
msgstr ""

#. type: SH
#: kill.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: kill.1:8
msgid "B<kill> [I<-s SIGNAL | -SIGNAL>] I<PID>..."
msgstr ""

#. type: Plain text
#: kill.1:11
msgid "B<kill> I<-l >[I<SIGNAL>]..."
msgstr ""

#. type: Plain text
#: kill.1:14
msgid "B<kill> I<-t >[I<SIGNAL>]..."
msgstr ""

#. type: SH
#: kill.1:14
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: kill.1:18
msgid "Send signals to processes, or list signals."
msgstr ""

#. type: Plain text
#: kill.1:20
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: Plain text
#: kill.1:22
msgid "B<-s>, B<--signal>=I<SIGNAL>, B<-SIGNAL>"
msgstr ""

#. type: Plain text
#: kill.1:24
msgid "specify the name or number of the signal to be sent"
msgstr ""

#. type: TP
#: kill.1:24
#, no-wrap
msgid "B<-l>, B<--list>"
msgstr ""

#. type: Plain text
#: kill.1:27
msgid "list signal names, or convert signal names to/from numbers"
msgstr ""

#. type: TP
#: kill.1:27
#, no-wrap
msgid "B<-t>, B<--table>"
msgstr ""

#. type: Plain text
#: kill.1:30
msgid "print a table of signal information"
msgstr ""

#. type: TP
#: kill.1:30
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: kill.1:33
msgid "display this help and exit"
msgstr ""

#. type: TP
#: kill.1:33
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: kill.1:36
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: kill.1:40
msgid ""
"SIGNAL may be a signal name like 'HUP', or a signal number like '1', or the "
"exit status of a process terminated by a signal.  PID is an integer; if "
"negative it identifies a process group."
msgstr ""

#. type: Plain text
#: kill.1:44
msgid ""
"NOTE: your shell may have its own version of kill, which usually supersedes "
"the version described here.  Please refer to your shell's documentation for "
"details about the options it supports."
msgstr ""

