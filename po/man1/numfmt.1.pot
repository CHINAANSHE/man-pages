#, no-wrap
msgid "NUMFMT"
msgstr ""

#. type: TH
#: numfmt.1:2
#, no-wrap
msgid "October 2013"
msgstr ""

#. type: TH
#: numfmt.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
