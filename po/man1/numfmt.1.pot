#. type: TH
#: numfmt.1:2
#, no-wrap
msgid "User Commands"
msgstr ""

#. type: SH
#: numfmt.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: numfmt.1:5
msgid "numfmt - Convert numbers from/to human-readable strings"
msgstr ""

#. type: SH
#: numfmt.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: numfmt.1:8
msgid "B<numfmt> [I<OPTION>]... [I<NUMBER>]..."
msgstr ""

#. type: SH
#: numfmt.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: numfmt.1:12
msgid ""
"Reformat NUMBER(s), or the numbers from standard input if none are specified."
msgstr ""

#. type: Plain text
#: numfmt.1:14
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: numfmt.1:14
#, no-wrap
msgid "B<--debug>"
msgstr ""

#. type: Plain text
#: numfmt.1:17
msgid "print warnings about invalid input"
msgstr ""

#. type: TP
#: numfmt.1:17
#, no-wrap
msgid "B<-d>, B<--delimiter>=I<X>"
msgstr ""

#. type: Plain text
#: numfmt.1:20
msgid "use X instead of whitespace for field delimiter"
msgstr ""

#. type: TP
#: numfmt.1:20
#, no-wrap
msgid "B<--field>=I<N>"
msgstr ""

#. type: Plain text
#: numfmt.1:23
msgid "replace the number in input field N (default is 1)"
msgstr ""

#. type: TP
#: numfmt.1:23
#, no-wrap
msgid "B<--format>=I<FORMAT>"
msgstr ""

#. type: Plain text
#: numfmt.1:27
msgid "use printf style floating-point FORMAT; see FORMAT below for details"
msgstr ""

#. type: TP
#: numfmt.1:27
#, no-wrap
msgid "B<--from>=I<UNIT>"
msgstr ""

#. type: Plain text
#: numfmt.1:31
msgid "auto-scale input numbers to UNITs; default is 'none'; see UNIT below"
msgstr ""

#. type: TP
#: numfmt.1:31
#, no-wrap
msgid "B<--from-unit>=I<N>"
msgstr ""

#. type: Plain text
#: numfmt.1:34
msgid "specify the input unit size (instead of the default 1)"
msgstr ""

#. type: TP
#: numfmt.1:34
#, no-wrap
msgid "B<--grouping>"
msgstr ""

#. type: Plain text
#: numfmt.1:38
msgid ""
"use locale-defined grouping of digits, e.g. 1,000,000 (which means it has no "
"effect in the C/POSIX locale)"
msgstr ""

#. type: TP
#: numfmt.1:38
#, no-wrap
msgid "B<--header>[=I<N>]"
msgstr ""

#. type: Plain text
#: numfmt.1:42
msgid ""
"print (without converting) the first N header lines; N defaults to 1 if not "
"specified"
msgstr ""

#. type: TP
#: numfmt.1:42
#, no-wrap
msgid "B<--invalid>=I<MODE>"
msgstr ""

#. type: Plain text
#: numfmt.1:46
msgid ""
"failure mode for invalid numbers: MODE can be: abort (default), fail, warn, "
"ignore"
msgstr ""

#. type: TP
#: numfmt.1:46
#, no-wrap
msgid "B<--padding>=I<N>"
msgstr ""

#. type: Plain text
#: numfmt.1:53
msgid ""
"pad the output to N characters; positive N will right-align; negative N will "
"left-align; padding is ignored if the output is wider than N; the default is "
"to automatically pad if a whitespace is found"
msgstr ""

#. type: TP
#: numfmt.1:53
#, no-wrap
msgid "B<--round>=I<METHOD>"
msgstr ""

#. type: Plain text
#: numfmt.1:57
msgid ""
"use METHOD for rounding when scaling; METHOD can be: up, down, from-zero "
"(default), towards-zero, nearest"
msgstr ""

#. type: TP
#: numfmt.1:57
#, no-wrap
msgid "B<--suffix>=I<SUFFIX>"
msgstr ""

#. type: Plain text
#: numfmt.1:61
msgid ""
"add SUFFIX to output numbers, and accept optional SUFFIX in input numbers"
msgstr ""

#. type: TP
#: numfmt.1:61
#, no-wrap
msgid "B<--to>=I<UNIT>"
msgstr ""

#. type: Plain text
#: numfmt.1:64
msgid "auto-scale output numbers to UNITs; see UNIT below"
msgstr ""

#. type: TP
#: numfmt.1:64
#, no-wrap
msgid "B<--to-unit>=I<N>"
msgstr ""

#. type: Plain text
#: numfmt.1:67
msgid "the output unit size (instead of the default 1)"
msgstr ""

#. type: TP
#: numfmt.1:67
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: numfmt.1:70
msgid "display this help and exit"
msgstr ""

#. type: TP
#: numfmt.1:70
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: numfmt.1:73
msgid "output version information and exit"
msgstr ""

#. type: SS
#: numfmt.1:73
#, no-wrap
msgid "UNIT options:"
msgstr ""

#. type: TP
#: numfmt.1:74
#, no-wrap
msgid "none"
msgstr ""

#. type: Plain text
#: numfmt.1:77
msgid "no auto-scaling is done; suffixes will trigger an error"
msgstr ""

#. type: TP
#: numfmt.1:77
#, no-wrap
msgid "auto"
msgstr ""

#. type: Plain text
#: numfmt.1:80
msgid "accept optional single/two letter suffix:"
msgstr ""

#. type: Plain text
#: numfmt.1:85
msgid "1K = 1000, 1Ki = 1024, 1M = 1000000, 1Mi = 1048576,"
msgstr ""

#. type: TP
#: numfmt.1:85
#, no-wrap
msgid "si"
msgstr ""

#. type: Plain text
#: numfmt.1:88 numfmt.1:95
msgid "accept optional single letter suffix:"
msgstr ""

#. type: Plain text
#: numfmt.1:92
msgid "1K = 1000, 1M = 1000000, \\&..."
msgstr ""

#. type: TP
#: numfmt.1:92
#, no-wrap
msgid "iec"
msgstr ""

#. type: Plain text
#: numfmt.1:99
msgid "1K = 1024, 1M = 1048576, \\&..."
msgstr ""

#. type: TP
#: numfmt.1:99
#, no-wrap
msgid "iec-i"
msgstr ""

#. type: Plain text
#: numfmt.1:102
msgid "accept optional two-letter suffix:"
msgstr ""

#. type: Plain text
#: numfmt.1:106
msgid "1Ki = 1024, 1Mi = 1048576, \\&..."
msgstr ""

#. type: Plain text
#: numfmt.1:111
msgid ""
"FORMAT must be suitable for printing one floating-point argument '%f'.  "
"Optional quote (%'f) will enable B<--grouping> (if supported by current "
"locale).  Optional width value (%10f) will pad output. Optional negative "
"width values (%-10f) will left-pad output."
msgstr ""

#. type: Plain text
#: numfmt.1:118
msgid ""
"Exit status is 0 if all input numbers were successfully converted.  By "
"default, numfmt will stop at the first conversion error with exit status 2.  "
"With B<--invalid=>'fail' a warning is printed for each conversion error and "
"the exit status is 2.  With B<--invalid=>'warn' each conversion error is "
"diagnosed, but the exit status is 0.  With B<--invalid=>'ignore' conversion "
"errors are not diagnosed and the exit status is 0."
msgstr ""

#. type: SH
#: numfmt.1:118
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: numfmt.1:121
msgid "CW<$ numfmt --to=si 1000>"
msgstr ""

#. type: Plain text
#: numfmt.1:123
msgid "-E<gt> \"1.0K\""
msgstr ""

#. type: Plain text
#: numfmt.1:125
msgid "CW<$ numfmt --to=iec 2048>"
msgstr ""

#. type: Plain text
#: numfmt.1:127
msgid "-E<gt> \"2.0K\""
msgstr ""

#. type: Plain text
#: numfmt.1:129
msgid "CW<$ numfmt --to=iec-i 4096>"
msgstr ""

#. type: Plain text
#: numfmt.1:131
msgid "-E<gt> \"4.0Ki\""
msgstr ""

#. type: Plain text
#: numfmt.1:133
msgid "CW<$ echo 1K | numfmt --from=si>"
msgstr ""

#. type: Plain text
#: numfmt.1:135
msgid "-E<gt> \"1000\""
msgstr ""

#. type: Plain text
#: numfmt.1:137
msgid "CW<$ echo 1K | numfmt --from=iec>"
msgstr ""

#. type: Plain text
#: numfmt.1:139
msgid "-E<gt> \"1024\""
msgstr ""

#. type: Plain text
#: numfmt.1:141
msgid "CW<$ df | numfmt --header --field 2 --to=si>"
msgstr ""

#. type: Plain text
#: numfmt.1:143
msgid "CW<$ ls -l | numfmt --header --field 5 --to=iec>"
msgstr ""

#. type: Plain text
#: numfmt.1:145
msgid "CW<$ ls -lh | numfmt --header --field 5 --from=iec --padding=10>"
msgstr ""

#. type: Plain text
#: numfmt.1:147
msgid "CW<$ ls -lh | numfmt --header --field 5 --from=iec --format %10f>"
msgstr ""

