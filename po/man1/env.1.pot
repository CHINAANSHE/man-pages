#. type: SH
#: env.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: env.1:5
msgid "env - run a program in a modified environment"
msgstr ""

#. type: SH
#: env.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: env.1:8
msgid "B<env> [I<OPTION>]... [I<->] [I<NAME=VALUE>]... [I<COMMAND >[I<ARG>]...]"
msgstr ""

#. type: SH
#: env.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: env.1:12
msgid "Set each NAME to VALUE in the environment and run COMMAND."
msgstr ""

#. type: Plain text
#: env.1:14
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: env.1:14
#, no-wrap
msgid "B<-i>, B<--ignore-environment>"
msgstr ""

#. type: Plain text
#: env.1:17
msgid "start with an empty environment"
msgstr ""

#. type: TP
#: env.1:17
#, no-wrap
msgid "B<-0>, B<--null>"
msgstr ""

#. type: Plain text
#: env.1:20
msgid "end each output line with 0 byte rather than newline"
msgstr ""

#. type: TP
#: env.1:20
#, no-wrap
msgid "B<-u>, B<--unset>=I<NAME>"
msgstr ""

#. type: Plain text
#: env.1:23
msgid "remove variable from the environment"
msgstr ""

#. type: TP
#: env.1:23
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: env.1:26
msgid "display this help and exit"
msgstr ""

#. type: TP
#: env.1:26
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: env.1:29
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: env.1:31
msgid "A mere - implies B<-i>.  If no COMMAND, print the resulting environment."
msgstr ""

