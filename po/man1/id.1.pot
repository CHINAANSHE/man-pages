msgid "User Commands"
msgstr ""

#. type: SH
#: id.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: id.1:5
msgid "id - print real and effective user and group IDs"
msgstr ""

#. type: SH
#: id.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: id.1:8
msgid "B<id> [I<OPTION>]... [I<USERNAME>]"
msgstr ""

#. type: SH
#: id.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: id.1:13
msgid ""
"Print user and group information for the specified USERNAME, or (when "
"USERNAME omitted) for the current user."
msgstr ""

#. type: TP
#: id.1:13
#, no-wrap
msgid "B<-a>"
msgstr ""

#. type: Plain text
#: id.1:16
msgid "ignore, for compatibility with other versions"
msgstr ""

#. type: TP
#: id.1:16
#, no-wrap
msgid "B<-Z>, B<--context>"
msgstr ""

#. type: Plain text
#: id.1:19
msgid "print only the security context of the current user"
msgstr ""

#. type: TP
#: id.1:19
#, no-wrap
msgid "B<-g>, B<--group>"
msgstr ""

#. type: Plain text
#: id.1:22
msgid "print only the effective group ID"
msgstr ""

#. type: TP
#: id.1:22
#, no-wrap
msgid "B<-G>, B<--groups>"
msgstr ""

#. type: Plain text
#: id.1:25
msgid "print all group IDs"
msgstr ""

#. type: TP
#: id.1:25
#, no-wrap
msgid "B<-n>, B<--name>"
msgstr ""

#. type: Plain text
#: id.1:28
msgid "print a name instead of a number, for B<-ugG>"
msgstr ""

#. type: TP
#: id.1:28
#, no-wrap
msgid "B<-r>, B<--real>"
msgstr ""

#. type: Plain text
#: id.1:31
msgid "print the real ID instead of the effective ID, with B<-ugG>"
msgstr ""

#. type: TP
#: id.1:31
#, no-wrap
msgid "B<-u>, B<--user>"
msgstr ""

#. type: Plain text
#: id.1:34
msgid "print only the effective user ID"
msgstr ""

#. type: TP
#: id.1:34
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: id.1:37
msgid "display this help and exit"
msgstr ""

#. type: TP
#: id.1:37
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: id.1:40
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: id.1:42
msgid "Without any OPTION, print some useful set of identified information."
msgstr ""

