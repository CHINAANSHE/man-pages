#. type: Plain text
#: factor.1:5
msgid "factor - factor numbers"
msgstr "factor  -  分解因数"

#. type: SH
#: factor.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: factor.1:8
msgid "B<factor> [I<NUMBER>]..."
msgstr "B<factor> [I<数字>]..."

#. type: Plain text
#: factor.1:11
msgid "B<factor> I<OPTION>"
msgstr "B<factor> I<选项>"

#. type: SH
#: factor.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: factor.1:16
msgid "Print the prime factors of each specified integer NUMBER.  If none are specified on the command line, read them from standard input."
msgstr "打印出一个特定整数的基本因数。如果没有在命令指定数字，那么将会从标准输入中获取。"

#. type: TP
#: factor.1:16
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: factor.1:19
msgid "display this help and exit"
msgstr "显示帮助信息并退出"

#. type: TP
#: factor.1:19
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: factor.1:22
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: SH
#: factor.1:22
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: factor.1:24
msgid "Written by Paul Rubin, Torbjorn Granlund, and Niels Moller."
msgstr "由Paul Rubin, Torbjorn Granlund, 和 Niels Moller撰写。"

#. type: SH
#: factor.1:24
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: factor.1:26
msgid "Report factor bugs to bug-coreutils@gnu.org"
msgstr "发送错误信息到： bug-coreutils@gnu.org"

#. type: Plain text
#: factor.1:28
msgid "GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
