#. type: TH
#: echo.1:2
#, no-wrap
msgid "ECHO"
msgstr "ECHO"

#. type: TH
#: echo.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: echo.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GUN coreutils 8.21"

#. type: TH
#: echo.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: echo.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: echo.1:5
msgid "echo - display a line of text"
msgstr "echo - 显示一行文本 "

#. type: SH
#: echo.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "摘要"

#. type: Plain text
#: echo.1:8
msgid "B<echo> [I<SHORT-OPTION>]... [I<STRING>]..."
msgstr "B<echo> [I<短选项>]...[I<字符串>]"

#. type: Plain text
#: echo.1:11
msgid "B<echo> I<LONG-OPTION>"
msgstr "B<echo> I< 长选项 >"

#. type: SH
#: echo.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: echo.1:15
msgid "Echo the STRING(s) to standard output."
msgstr "将字符串 STRING(s) 显示到标准输出"

#. type: TP
#: echo.1:15
#, no-wrap
msgid "B<-n>"
msgstr "B<-n>"

#. type: Plain text
#: echo.1:18
msgid "do not output the trailing newline"
msgstr "不输出最后的换行符"

#. type: TP
#: echo.1:18
#, no-wrap
msgid "B<-e>"
msgstr "B<-e>"

#. type: Plain text
#: echo.1:21
msgid "enable interpretation of backslash escapes"
msgstr "使能对转义字符的解释"

#. type: TP
#: echo.1:21
#, no-wrap
msgid "B<-E>"
msgstr "B<-E>"

#. type: Plain text
#: echo.1:24
msgid "disable interpretation of backslash escapes (default)"
msgstr "禁用对转义字符的解释(默认)"

#. type: TP
#: echo.1:24
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: echo.1:27
msgid "display this help and exit"
msgstr "显示帮助并退出"

#. type: TP
#: echo.1:27
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: echo.1:30
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#. type: Plain text
#: echo.1:32
msgid "If B<-e> is in effect, the following sequences are recognized:"
msgstr "如果 B<-e> 有效，下面的序列将会被识别"

#. type: TP
#: echo.1:32
#, no-wrap
msgid "\\e\\e"
msgstr "\\e\\e"

#. type: Plain text
#: echo.1:35
msgid "backslash"
msgstr "反斜杠"

#. type: TP
#: echo.1:35
#, no-wrap
msgid "\\ea"
msgstr "\\ea"

#. type: Plain text
#: echo.1:38
msgid "alert (BEL)"
msgstr "报警（ BEL ）"

#. type: TP
#: echo.1:38
#, no-wrap
msgid "\\eb"
msgstr "\\eb"

#. type: Plain text
#: echo.1:41
msgid "backspace"
msgstr "退格"

#. type: TP
#: echo.1:41
#, no-wrap
msgid "\\ec"
msgstr "\\ec"

#. type: Plain text
#: echo.1:44
msgid "produce no further output"
msgstr "不再产生输出"

#. type: TP
#: echo.1:44
#, no-wrap
msgid "\\ee"
msgstr "\\ee"

#. type: Plain text
#: echo.1:47
msgid "escape"
msgstr "退出"

#. type: TP
#: echo.1:47
#, no-wrap
msgid "\\ef"
msgstr "\\ef"

#. type: Plain text
#: echo.1:50
msgid "form feed"
msgstr "换页"

#. type: TP
#: echo.1:50
#, no-wrap
msgid "\\en"
msgstr "\\en"

#. type: Plain text
#: echo.1:53
msgid "new line"
msgstr "换行"

#. type: TP
#: echo.1:53
#, no-wrap
msgid "\\er"
msgstr "\\er"

#. type: Plain text
#: echo.1:56
msgid "carriage return"
msgstr "回车"

#. type: TP
#: echo.1:56
#, no-wrap
msgid "\\et"
msgstr "\\et"

#. type: Plain text
#: echo.1:59
msgid "horizontal tab"
msgstr "纵向制表"

#. type: TP
#: echo.1:59
#, no-wrap
msgid "\\ev"
msgstr "\\ev"

#. type: Plain text
#: echo.1:62
msgid "vertical tab"
msgstr "横向制表"

#. type: TP
#: echo.1:62
#, no-wrap
msgid "\\e0NNN"
msgstr "\\e0NNN"

#. type: Plain text
#: echo.1:65
msgid "byte with octal value NNN (1 to 3 digits)"
msgstr "用八进制数 NNN （1 到 3 位）表示的 ASCII 字节"

#. type: TP
#: echo.1:65
#, no-wrap
msgid "\\exHH"
msgstr "\\exHH"

#. type: Plain text
#: echo.1:68
msgid "byte with hexadecimal value HH (1 to 2 digits)"
msgstr "用十六进制数 HH （1 到 2 位）表示的 ASCII 字节"

#. type: Plain text
#: echo.1:72
msgid "NOTE: your shell may have its own version of echo, which usually supersedes the version described here.  Please refer to your shell's documentation for details about the options it supports."
msgstr "提醒：你的 shell 可能有其自己版本的 echo 命令。该命令通常会取代此处描述的版本 。具体请参阅你的 shell 的文档以了解它所支持的选项。"

#. type: SH
#: echo.1:72
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: echo.1:74
msgid "Written by Brian Fox and Chet Ramey."
msgstr "由 Brian Fox 和 Chet Ramey 撰写"

#. type: SH
#: echo.1:74
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告错误"

#. type: Plain text
#: echo.1:76
msgid "Report echo bugs to bug-coreutils@gnu.org"
msgstr "发送错误信息到：bug-coreutils@gnu.org"

#. type: Plain text
#: echo.1:78
msgid "GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页： E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: echo.1:80
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "使用GNU软件的常规帮助： E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: echo.1:82
msgid "Report echo translation bugs to E<lt>http://github.com/LCTT/man-pages/E<gt>"
msgstr "发送翻译错误到 E<lt>http://github.com/LCTT/man-pages/E<gt>"

#. type: SH
#: echo.1:82
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权声明"

#. type: Plain text
#: echo.1:85
msgid "Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr "Copyright \\(co 2013 Free Software Foundation, Inc. License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gplhtmlE<gt>."

#. type: Plain text
#: echo.1:88
msgid "This is free software: you are free to change and redistribute it.  There is NO WARRANTY, to the extent permitted by law."
msgstr "这是一个自由软件： 你可以自由修改和重新发布它。 在法律允许的范围内， 不提供任何担保"

#. type: SH
#: echo.1:88
#, no-wrap
msgid "SEE ALSO"
msgstr "请参阅"

#. type: Plain text
#: echo.1:96
msgid "The full documentation for B<echo> is maintained as a Texinfo manual.  If the B<info> and B<echo> programs are properly installed at your site, the command"
msgstr "本程序的完整文档使用 Texinfo 手册进行维护。如果 B<info> 和本程序已经正确地安装在了你的电脑上，通过下述命令"

#. type: Plain text
#: echo.1:98
msgid "B<info coreutils \\(aqecho invocation\\(aq>"
msgstr "B<info coreutils \\(aqecho invocation\\(aq>"

#. type: Plain text
#: echo.1:100
msgid "should give you access to the complete manual."
msgstr "可以访问完整的使用手册"

