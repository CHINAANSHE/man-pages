#. type: TH
#: cat.1:2
#, no-wrap
msgid "CAT"
msgstr "CAT"

#. type: TH
#: cat.1:2
#, no-wrap
msgid "October 2013"
msgstr "2013年10月"

#. type: TH
#: cat.1:2
#, no-wrap
msgid "GNU coreutils 8.21"
msgstr "GNU coreutils 8.21"

#. type: TH
#: cat.1:2
#, no-wrap
msgid "User Commands"
msgstr "用户命令"

#. type: SH
#: cat.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: cat.1:5
msgid "cat - concatenate files and print on the standard output"
msgstr "cat-连接文件并在标准输出上打印"

#. type: SH
#: cat.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "简介"

#. type: Plain text
#: cat.1:8
msgid "B<cat> [I<OPTION>]... [I<FILE>]..."
msgstr "B<cat> [I<选项>]... [I<文件>]..."

#. type: SH
#: cat.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "说明"

#. type: Plain text
#: cat.1:12
msgid "Concatenate FILE(s), or standard input, to standard output."
msgstr "连接文件，或标准输入到标准输出"

# 这个show-all要不要翻译呀
#. type: TP
#: cat.1:12
#, no-wrap
msgid "B<-A>, B<--show-all>"
msgstr "B<-A>, B<--show-all>"

#. type: Plain text
#: cat.1:15
msgid "equivalent to B<-vET>"
msgstr "相当于 B<-vET>"

#. type: TP
#: cat.1:15
#, no-wrap
msgid "B<-b>, B<--number-nonblank>"
msgstr "B<-b>, B<--number-nonblank>"

#. type: Plain text
#: cat.1:18
msgid "number nonempty output lines, overrides B<-n>"
msgstr "空行不输出编号，重写 B<-n>"

#. type: TP
#: cat.1:18
#, no-wrap
msgid "B<-e>"
msgstr "B<-e>"

#. type: Plain text
#: cat.1:21
msgid "equivalent to B<-vE>"
msgstr "相当于 B<-vE>"

#. type: TP
#: cat.1:21
#, no-wrap
msgid "B<-E>, B<--show-ends>"
msgstr "B<-E>, B<--show-ends>"

#. type: Plain text
#: cat.1:24
msgid "display $ at end of each line"
msgstr "在每一行结尾显示$"

#. type: TP
#: cat.1:24
#, no-wrap
msgid "B<-n>, B<--number>"
msgstr "B<-n>, B<--number>"

#. type: Plain text
#: cat.1:27
msgid "number all output lines"
msgstr "所有行都输出编号"

#. type: TP
#: cat.1:27
#, no-wrap
msgid "B<-s>, B<--squeeze-blank>"
msgstr "B<-s>, B<--squeeze-blank>"

#. type: Plain text
#: cat.1:30
msgid "suppress repeated empty output lines"
msgstr "禁止输出连续的重复空行"

#. type: TP
#: cat.1:30
#, no-wrap
msgid "B<-t>"
msgstr "B<-t>"

#. type: Plain text
#: cat.1:33
msgid "equivalent to B<-vT>"
msgstr "相当于B<-vT>"

#. type: TP
#: cat.1:33
#, no-wrap
msgid "B<-T>, B<--show-tabs>"
msgstr "B<-T>, B<--show-tabs>"

#. type: Plain text
#: cat.1:36
msgid "display TAB characters as ^I"
msgstr "显示TAB字符为^I"

#. type: TP
#: cat.1:36
#, no-wrap
msgid "B<-u>"
msgstr "B<-u>"

#. type: Plain text
#: cat.1:39
msgid "(ignored)"
msgstr "(忽略)"

#. type: TP
#: cat.1:39
#, no-wrap
msgid "B<-v>, B<--show-nonprinting>"
msgstr "B<-v>, B<--show-nonprinting>"

#. type: Plain text
#: cat.1:42
msgid "use ^ and M- notation, except for LFD and TAB"
msgstr "使用^和M-记号，除了LFD和TAB"

#. type: TP
#: cat.1:42
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: cat.1:45
msgid "display this help and exit"
msgstr "显示帮助并且退出"

#. type: TP
#: cat.1:45
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: cat.1:48
msgid "output version information and exit"
msgstr "输出版本信息并且退出"

#. type: Plain text
#: cat.1:50
msgid "With no FILE, or when FILE is -, read standard input."
msgstr "没有文件或当文件是-时，读取标准输入"

#. type: SH
#: cat.1:50
#, no-wrap
msgid "EXAMPLES"
msgstr "例子"

#. type: TP
#: cat.1:51
#, no-wrap
msgid "cat f - g"
msgstr "cat f - g"

#. type: Plain text
#: cat.1:54
msgid "Output f's contents, then standard input, then g's contents."
msgstr "输出f's内容，然后标准输入，加上g's内容"

#. type: TP
#: cat.1:54
#, no-wrap
msgid "cat"
msgstr "cat"

#. type: Plain text
#: cat.1:57
msgid "Copy standard input to standard output."
msgstr "复制标准输入到标准输出"

#. type: SH
#: cat.1:57
#, no-wrap
msgid "AUTHOR"
msgstr "作者"

#. type: Plain text
#: cat.1:59
msgid "Written by Torbjorn Granlund and Richard M. Stallman."
msgstr "由Torbjorn Granlund和Richard M. Stallman书写"

#. type: SH
#: cat.1:59
#, no-wrap
msgid "REPORTING BUGS"
msgstr "报告BUGS"

#. type: Plain text
#: cat.1:61
msgid "Report cat bugs to bug-coreutils@gnu.org"
msgstr "报告cat bugs到bug-coreutils@gnu.org"

#. type: Plain text
#: cat.1:63
msgid "GNU coreutils home page: E<lt>http://www.gnu.org/software/coreutils/E<gt>"
msgstr "GNU coreutils 主页: E<lt>http://www.gnu.org/software/coreutils/E<gt>"

#. type: Plain text
#: cat.1:65
msgid "General help using GNU software: E<lt>http://www.gnu.org/gethelp/E<gt>"
msgstr "一般使用GNU软件的帮助: E<lt>http://www.gnu.org/gethelp/E<gt>"

#. type: Plain text
#: cat.1:67
msgid "Report cat translation bugs to E<lt>http://translationproject.org/team/E<gt>"
msgstr "报告cat翻译bugs到 E<lt>http://translationproject.org/team/E<gt>"

#. type: SH
#: cat.1:67
#, no-wrap
msgid "COPYRIGHT"
msgstr "版权所有"

#. type: Plain text
#: cat.1:70
msgid "Copyright \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."
msgstr "版权所有 \\(co 2013 Free Software Foundation, Inc.  License GPLv3+: GNU GPL version 3 or later E<lt>http://gnu.org/licenses/gpl.htmlE<gt>."

#. type: Plain text
#: cat.1:73
msgid "This is free software: you are free to change and redistribute it.  There is NO WARRANTY, to the extent permitted by law."
msgstr "这是自由软件：你可以自由更改并重新发布它。这是没有授权的，在法律所允许的范围内"

#. type: SH
#: cat.1:73
#, no-wrap
msgid "SEE ALSO"
msgstr "请参阅"

#. type: Plain text
#: cat.1:75
msgid "B<tac>(1)"
msgstr "B<tac>(1)"

#. type: Plain text
#: cat.1:83
msgid "The full documentation for B<cat> is maintained as a Texinfo manual.  If the B<info> and B<cat> programs are properly installed at your site, the command"
msgstr "B<cat>的完整文档作为Texinfo手册进行维护。如果 B<info> 和B<cat>程序已正确安装在你的站点，此命令"

#. type: Plain text
#: cat.1:85
msgid "B<info coreutils \\(aqcat invocation\\(aq>"
msgstr "B<info coreutils \\(aqcat invocation\\(aq>"

#. type: Plain text
#: cat.1:86
msgid "should give you access to the complete manual."
msgstr "应给予你访问完整的手册"

